#tag ModuleProtected Module SystemExtensions	#tag Method, Flags = &h0		Function SystemFinderLabelColors() As Color()		  //#Returns the Finder labels as an array of strings		  		  #if TargetMacOS		    dim colors() as NSColor = NSWorkspace.SharedWorkspace.FinderLabelColors		    dim returnValue() as Color		    for each colour as NSColor in colors		      returnValue.append colour		    next		    returnValue.Remove  0  //Remove the first label color which actually corresponds to "no label" (defined as black)		    return  returnValue		  #endif		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function SystemFinderLabels() As string()		  //#Returns the Finder labels as an array of strings		  		  #if TargetMacOS		    dim labels() as String = NSWorkspace.SharedWorkspace.FinderLabels		    		    labels.Remove  0  //Remove the first label which actually corresponds to "no label"		    return  labels		  #endif		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function SystemListPrinterNames() As string()		  //# Returns the list of the installed printers name		  		  return   NSPrinter.Names		  		End Function	#tag EndMethod	#tag Method, Flags = &h0		Function SystemListPrinterTypes() As string()		  //# Returns the list of the installed printers type		  		  return   NSPrinter.Types		  		  		End Function	#tag EndMethod	#tag ViewBehavior		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			InitialValue="-2147483648"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Object"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			InitialValue="0"			InheritedFrom="Object"		#tag EndViewProperty	#tag EndViewBehaviorEnd Module#tag EndModule