.DrawPicture pic, 15, me.Height - pic.Height - 5		End Sub	#tag EndEvent#tag EndEvents#tag Events lblAppName	#tag Event		Sub Open()		  me.text = app.kAppName		End Sub	#tag EndEvent#tag EndEvents#tag Events lblVersion	#tag Event		Sub Open()		  if app.BugVersion > 0 then		    LongVersion = str(app.MajorVersion) + "." + str(app.MinorVersion) + "." + str(app.BugVersion)		  else		    LongVersion = str(app.MajorVersion) + "." + str(app.MinorVersion)		  end if		  		  me.text = LongVersion		  		  if DebugBuild then		    if app.NonReleaseVersion > 0 then		      If app.StageCode = 0 then		        me.text = me.text + "R" + str(app.NonReleaseVersion)		      ElseIf app.StageCode = 1 then		        me.text = me.text + "A" + str(app.NonReleaseVersion)		      ElseIf app.StageCode = 2 then		        me.text = me.text + "B" + str(app.NonReleaseVersion)		      ElseIf app.StageCode = 3 then		        'me.text = me.text + " (Final)"		      end if // @END Stagecode		      me.text = me.text + " (debug)"		    end if // @END NonReleaseVersion		  end if // @END DebugBuild		End Sub	#tag EndEvent#tag EndEvents#tag Events lblCopyright	#tag Event		Sub Open()		  Dim d as new date		  if d.Year > kYear then		    me.text = me.Text.ReplaceAll( "<year>", str( kYear ) + "-" + str( d.Year ) )		  else		    me.text = me.Text.ReplaceAll( "<year>", str( kYear ) )		  end if		  		  me.text = me.Text.ReplaceAll( "<company>", app.kCompany )		  me.text = me.Text.ReplaceAll( "<AppName>", app.kAppName )		End Sub	#tag EndEvent#tag EndEvents#tag Events lblUpdateStatus	#tag Event		Sub Open()		  me.text = me.text.ReplaceAll( "<AppName>", app.kAppName )		End Sub	#tag EndEvent	#tag Event		Function MouseDown(X As Integer, Y As Integer) As Boolean		  Return True		End Function	#tag EndEvent	#tag Event		Sub MouseUp(X As Integer, Y As Integer)		  if Glimmer.NewVersion <> Glimmer.CurrentVersion and Preferences.SkipVersion <> Glimmer.NewVersion then		    wndUpdateAvailable.Show		    wndUpdateAvailable.HTMLReleaseNotes.LoadURL Glimmer.ReleasenotesLink		  end if		End Sub	#tag EndEvent	#tag Event		Sub MouseEnter()		  if Glimmer.NewVersion <> Glimmer.CurrentVersion and Preferences.SkipVersion <> Glimmer.NewVersion then me.Underline = False		End Sub	#tag EndEvent	#tag Event		Sub MouseExit()		  if Glimmer.NewVersion <> Glimmer.CurrentVersion and Preferences.SkipVersion <> Glimmer.NewVersion then me.Underline = True		End Sub	#tag EndEvent#tag EndEvents#tag Events btnClose	#tag Event		Sub Action()		  self.close		End Sub	#tag EndEvent	#tag Event		Sub Open()		  #if not TargetMacOS then		    me.Visible = True		  #else		    me.Visible = False		  #endif		End Sub	#tag EndEvent#tag EndEvents