 ExtraArt		  end if		End Function	#tag EndEvent	#tag Event		Sub Open()		  Closed = False		  		  tpMetadata.Value = Preferences.AnimeMetaDataWindowTab		  		  '#if TargetMacOS then SmoothResize( self, self.Width, WindowHeightDivider( tpMetadata.Value ).Top + 51, 4 )		  'self.Height = WindowHeightDivider( tpMetadata.Value ).top + 52		  		  DistributeData		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub DistributeData()		  Dim sGenre as String		  Dim Hours, Minutes, i as Integer		  		  if AnimeAttr.Title <> "" then		    self.Title = AnimeAttr.Title.ReplaceAll( "&", "&&" )		  else		    self.Title = wndMain.lstAnime.Cell( wndMain.lstAnime.ListIndex, 0 ).ReplaceAll( "&", "&&" )		  end if		  		  // Anime Info		  edtTitle.Text = AnimeAttr.Title		  edtTitleOriginal.Text = AnimeAttr.TitleOriginal		  edtTitleSort.Text = AnimeAttr.TitleSort		  edtPlot.Text = AnimeAttr.Plot		  edtReleaseDate.Text = AnimeAttr.DatePremiered		  edtYear.Text = AnimeAttr.Year		  edtStudio.Text = AnimeAttr.Studio		  if AnimeAttr.Rating < 0 then edtRating.Text = "0" else edtRating.Text = str( AnimeAttr.Rating )		  		  // Anime Summary Info		  lblTitle.Text = AnimeAttr.Title.ReplaceAll( "&", "&&" )		  'lblRuntimeValue.Text = str( AnimeAttr.RuntimeInMinutes )		  if AnimeAttr.RuntimeInMinutes < 61 then		    lblRuntimeValue.Text = str(AnimeAttr.RuntimeInMinutes) + " Minutes"		  else		    Hours = floor( AnimeAttr.RuntimeInMinutes / 60 )		    Minutes = AnimeAttr.RuntimeInMinutes - (60 * Hours)		    if hours < 2 and Minutes > 0 then		      lblRuntimeValue.Text = str(hours) + " Hour and " + str(Minutes) + " Minutes"		    elseif hours < 2 then		      lblRuntimeValue.Text = str(hours) + " Hour"		    elseif Minutes > 0 then		      lblRuntimeValue.Text = str(hours) + " Hours and " + str(Minutes) + " Minutes"		    else		      lblRuntimeValue.Text = str(hours) + " Hours"		    end if		  end if		  		  // TVDB & AniDB Label		  LinkAniDB.Text = AnimeAttr.ID_AniDB		  LinkAniDB.URL = "http://anidb.net/perl-bin/animedb.pl?show=anime&aid=" + AnimeAttr.ID_AniDB		  		  LinkTVDB.Text = AnimeAttr.ID_TVDb		  LinkTVDB.URL = "http://thetvdb.com/?tab=series&id=" + AnimeAttr.ID_TVDb		  		  // Related Anime		  for i = 0 to 5		    // quick reset		    lblRelatedAnime(i).Text = ""		  next		  		  if AnimeAttr.SimilarAnimeTitle.Ubound > -1 then		    for i = 0 to AnimeAttr.SimilarAnimeTitle.Ubound		      lblRelatedAnime(i).Text = AnimeAttr.SimilarAnimeTitle(i)		      lblRelatedAnime(i).URL = "http://anidb.net/perl-bin/animedb.pl?show=anime&aid=" + str( AnimeAttr.SimilarAnimeID(i) )		    next		  end if		  		  // File Path		  lblSummaryFilePath.Text = AnimeAttr.FolderParent.AbsolutePath		  		  Select case AnimeAttr.MPAA		    		  case ""		    popMPAARating.ListIndex = 0		  case "TV-G"		    popMPAARating.ListIndex = 1		  case "TV-PG"		    popMPAARating.ListIndex = 2		  case "TV-14"		    popMPAARating.ListIndex = 3		  case "TV-MA"		    popMPAARating.ListIndex = 4		  case "TV-Y"		    popMPAARating.ListIndex = 5		  case "TV-Y7"		    popMPAARating.ListIndex = 6		  case "TV-Y7FV"		    popMPAARating.ListIndex = 7		    		  end Select		  		  // Actor List		  lstActors.DeleteAllRows		  for i = 0 to AnimeAttr.ActorName.Ubound		    lstActors.AddRow AnimeAttr.ActorName(i), AnimeAttr.ActorRole(i), AnimeAttr.ActorThumbURL(i), AnimeAttr.ActorRoleDescription(i)		  next		  		  // Genres		  for i = 0 to AnimeAttr.Genre.Ubound		    if i = 0 then		      sGenre = AnimeAttr.Genre(i)		    elseif i = AnimeAttr.Genre.Ubound then		      sGenre = sGenre + ", " + AnimeAttr.Genre(i)		    else		      sGenre = sGenre + ", " + AnimeAttr.Genre(i)		    end if		  next		  edtGenres.Text = sGenre		  		  // Prev & Next Buttons		  if wndMain.ppMedia.Value = 2 and wndMain.lstAnime.ListIndex = wndMain.lstAnime.ListCount - 1 then btnNext.Enabled = False else btnNext.Enabled = True		  if wndMain.ppMedia.Value = 2 and wndMain.lstAnime.ListIndex = 0 then btnPrev.Enabled = False else btnPrev.Enabled = True		  		  // Canvas'		  cvsActor.Refresh(false)		  cvsBannerFrame.Refresh(False)		  cvsExtras.Refresh(false)		  cvsFanartFrame.Refresh(false)		  cvsPosterFrame.Refresh(false)		  cvsSummaryPoster.Refresh(false)		  		  // TV Rage - Next Aired data		  lblNextAiredValue.Text = ""		  lblNextEpisodeValue.Text = ""		  pgWheelNextAired.Visible = True		  Scrape.TVRageNextAired( String2Entities( AnimeAttr.TitleOriginal.ReplaceAll("&", "and") ) )		  if NOT Closed then		    lblNextAiredValue.Text = AnimeAttr.NextAired		    lblNextAiredValue.HelpTag = AnimeAttr.NextAired		    lblNextEpisodeValue.Text = AnimeAttr.NextEpisode		    lblNextEpisodeValue.HelpTag = AnimeAttr.NextEpisode		    pgWheelNextAired.Visible = False		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub SaveData()		  Dim sGenres As String		  Dim i As Integer		  		  // Show Info		  AnimeAttr.Title = edtTitle.Text		  AnimeAttr.TitleOriginal = edtTitleOriginal.Text		  AnimeAttr.TitleSort = edtTitleSort.Text		  AnimeAttr.Plot = edtPlot.Text		  AnimeAttr.DatePremiered = edtReleaseDate.Text		  AnimeAttr.Year = edtYear.Text		  AnimeAttr.Rating = val( edtRating.Text )		  AnimeAttr.Studio = edtStudio.Text		  		  // MPAA Rating		  Select case popMPAARating.ListIndex		    		  case 0		    AnimeAttr.MPAA = ""		  case 1		    AnimeAttr.MPAA = "TV-G"		  case 2		    AnimeAttr.MPAA = "TV-PG"		  case 3		    AnimeAttr.MPAA = "TV-14"		  case 4		    AnimeAttr.MPAA = "TV-MA"		  case 5		    AnimeAttr.MPAA = "TV-Y"		  case 6		    AnimeAttr.MPAA = "TV-Y7"		  case 7		    AnimeAttr.MPAA = "TV-Y7FV"		    		  end Select		  		  ReDim AnimeAttr.ActorName(-1)		  ReDim AnimeAttr.ActorRole(-1)		  ReDim AnimeAttr.ActorThumbURL(-1)		  ReDim AnimeAttr.ActorRoleDescription(-1)		  for i = 0 to lstActors.ListCount - 1		    		    if lstActors.Cell( i, 0 ) <> "" then		      AnimeAttr.ActorName.Append lstActors.Cell( i, 0 )		    else		      AnimeAttr.ActorName.Append ""		    end if		    		    if lstActors.Cell( i, 1 ) <> "" then		      AnimeAttr.ActorRole.Append lstActors.Cell( i, 1 )		    else		      AnimeAttr.ActorRole.Append ""		    end if		    		    if lstActors.Cell( i, 2 ) <> "" then		      AnimeAttr.ActorThumbURL.Append lstActors.Cell( i, 2 )		    else		      AnimeAttr.ActorThumbURL.Append ""		    end if		    		    if lstActors.Cell( i, 3 ) <> "" then		      AnimeAttr.ActorRoleDescription.Append lstActors.Cell( i, 3 )		    else		      AnimeAttr.ActorRoleDescription.Append ""		    end if		    		  next		  		  // Genres		  sGenres = edtGenres.Text.ReplaceAll(", ", ",")		  AnimeAttr.Genre = sGenres.Split(",")		  		  // Save Data		  AnimeAttr.WriteInformation( AnimeAttr.FolderParent )		End Sub	#tag EndMethod	#tag Property, Flags = &h1		Protected ActorIMG As Picture	#tag EndProperty	#tag Property, Flags = &h1		Protected ActorLastSelection As Integer = -1	#tag EndProperty	#tag Property, Flags = &h0		Closed As Boolean	#tag EndProperty#tag EndWindowCode#tag Events tpMetadata	#tag Event		Sub Change()		  'Preferences.ShowMetaDataWindowTab = me.Value		  		  // #if TargetMacOS then self.SmoothResizeMBS( self.Width, WindowHeightDivider( me.Value ).top + 51 )		  #if TargetMacOS then SmoothResize( self, self.Width, WindowHeightDivider( me.Value ).Top + 51, 4 )		  self.Height = WindowHeightDivider( me.Value ).top + 52		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsSummaryPoster	#tag Event		Sub Paint(g As Graphics)		  g.ForeColor = &cFFFFFF		  g.FillRect 0, 0, me.Width, me.Height		  		  if AnimeAttr.ART_Poster <> nil then		    g.DrawPicture AnimeAttr.ART_Poster, 0, 0, me.Width, me.Height, 0, 0, AnimeAttr.ART_Poster.Width, AnimeAttr.ART_Poster.Height		  else		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Poster" + chr(13) + "Available", (me.Width / 2) - (g.StringWidth("No Poster" + chr(13) + "Available") / 4), (me.Height / 2) - (g.TextSize * 2), me.Width, False )		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent#tag EndEvents#tag Events edtRating	#tag Event		Function KeyDown(Key As String) As Boolean		  		  if Keyboard.AsyncKeyDown(&h7E) or Keyboard.AsyncKeyDown(&h7C) then		    if Val( edtRating.Text ) < 10 then edtRating.Text = str( val( edtRating.Text ) + 0.1 )		  ElseIf Keyboard.AsyncKeyDown(&h7B) or Keyboard.AsyncKeyDown(&h7D) then		    if Val( edtRating.Text ) > 0 then edtRating.Text = str( val( edtRating.Text ) - 0.1 )		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events udRating	#tag Event		Sub Down()		  if Val( edtRating.Text ) > 0 then edtRating.Text = str( val( edtRating.Text ) - 0.1 )		End Sub	#tag EndEvent	#tag Event		Sub Up()		  if Val( edtRating.Text ) < 10 then edtRating.Text = str( val( edtRating.Text ) + 0.1 )		End Sub	#tag EndEvent#tag EndEvents#tag Events lstActors	#tag Event		Sub Change()		  pgWheelActors.Visible = False		  		  if edtRoleDescription.Text <> "" then lstActors.Cell( ActorLastSelection, 3 ) = edtRoleDescription.Text		  if me.ListIndex > -1 then edtRoleDescription.Text = me.Cell( me.ListIndex, 3 )		  		  if me.ListIndex > -1 and me.ListIndex <> ActorLastSelection then		    ActorIMG = nil		    cvsActor.Refresh(false)		    		    if me.Cell(me.ListIndex, 2) <> ""  then		      pgWheelActors.Visible = true		      ActorTimer.Mode = 1		    end if		    		    ActorLastSelection = me.ListIndex		  end if		  		  if me.ListIndex > -1 then		    arActor.RemoveEnabled = True		  else		    arActor.RemoveEnabled = False		  end if		  arActor.Refresh(false)		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  if Keyboard.AsyncKeyDown(&h33) or Keyboard.AsyncKeyDown(&h75) then		    if me.active and me.listindex <> -1 then		      me.RemoveRow( me.listindex )		    end if		  end if		End Function	#tag EndEvent	#tag Event		Sub Open()		  me.ColumnType(0) = me.TypeEditable		  me.ColumnType(1) = me.TypeEditable		  me.ColumnType(2) = me.TypeEditable		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsActor	#tag Event		Sub Paint(g As Graphics)		  Dim pic as Picture		  		  g.ForeColor = &cFFFFFF		  g.FillRect 0, 0, me.Width, me.Height		  		  if ActorIMG <> nil then		    pic = ScalePic( ActorIMG, me.Width, me.Height, True, True, True )		    g.DrawPicture pic, 0, me.Height - pic.Height		  else		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Image" + chr(13) + "Available", (me.Width / 2) - (g.StringWidth("No Image" + chr(13) + "Available") / 4), (me.Height / 2) - (g.TextSize * 2), me.Width, False )		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent#tag EndEvents#tag Events ActorTimer	#tag Event		Sub Action()		  ActorSckt.Yield = true		  if lstActors.ListIndex > -1 then		    if lstActors.Cell( lstActors.ListIndex, 2 ).Left(4) = "http" then		      ActorIMG = Picture.FromData( ActorSckt.Get( lstActors.Cell( lstActors.ListIndex, 2 ), 30 ) )		      ActorSckt.close		    end if		    pgWheelActors.Visible = False		    cvsActor.Refresh(false)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events arActor	#tag Event		Sub Open()		  me.RemoveEnabled = false		  me.Refresh(false)		End Sub	#tag EndEvent	#tag Event		Sub Remove()		  if lstActors.ListIndex > -1 then lstActors.RemoveRow lstActors.ListIndex		End Sub	#tag EndEvent	#tag Event		Sub Activate()		  me.Refresh(false)		End Sub	#tag EndEvent	#tag Event		Sub Add()		  lstActors.AddRow edtActorName.text, edtActorRole.Text, edtActorThumbURL.Text		  lstActors.ListIndex = lstActors.LastIndex		  		  edtActorName.Text     = ""		  edtActorRole.Text     = ""		  edtActorThumbURL.Text = ""		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsPosterFrame	#tag Event		Sub Paint(g As Graphics)		  Dim pic as Picture		  Dim PicDimensions As String		  		  g.ForeColor = &c929292		  g.FillRect 0, 0, me.Width, me.Height		  		  if AnimeAttr.ART_Poster <> Nil then		    pic = ScalePic( AnimeAttr.ART_Poster, me.Width, me.Height )		    g.DrawPicture pic, (me.Width / 2) - (pic.Width / 2), (me.Height / 2) - (pic.Height / 2)		    		    // Sizes label ---------		    PicDimensions = "Size: " + str(AnimeAttr.ART_Poster.Width) + "x" + str(AnimeAttr.ART_Poster.Height)		    		    g.TextSize = 11		    g.Bold = True		    		    g.ForeColor = &CFFFFFF		    g.FillRect 3, 3, (PicDimensions.Len * 7), g.TextHeight + 10		    g.ForeColor = &c000000		    g.DrawRect 3, 3, (PicDimensions.Len * 7), g.TextHeight + 10		    g.DrawString( PicDimensions, 9, 8 + g.TextSize )		    		    pic = Nil		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent	#tag Event		Sub Open()		  		  me.AcceptFileDrop("image/png")		  me.AcceptFileDrop("image/jpg")		End Sub	#tag EndEvent	#tag Event		Sub DropObject(obj As DragItem, action As Integer)		  Dim PosterPath As FolderItem		  Dim Message, Explanation, PosterName as String		  		  Explanation = "If you choose 'OK' the existing image will be replaced with the new image."		  Message = "Poster image already exists, replace image?"		  		  PosterPath = AnimeAttr.ShowPosterFilter		  		  if obj.FolderItemAvailable and obj.FolderItem <> nil then		    		    if PosterPath <> nil and PosterPath.Exists then		      		      PosterName = PosterPath.Name.Left( PosterPath.Name.Len - 4 )		      		      if dlgWindow(Message, Explanation) then		        PosterPath.Delete		        if obj.FolderItem.Type = "image/png" then		          obj.FolderItem.CopyFileTo AnimeAttr.FolderParent.Child( PosterName + ".png")		        elseif obj.FolderItem.Type = "image/jpg" then		          obj.FolderItem.CopyFileTo AnimeAttr.FolderParent.Child( PosterName + ".jpg")		        end if		      end if		      		    else		      		      if obj.FolderItem.Type = "image/png" then		        obj.FolderItem.CopyFileTo AnimeAttr.FolderParent.Child("poster.png")		      elseif obj.FolderItem.Type = "image/jpg" then		        obj.FolderItem.CopyFileTo AnimeAttr.FolderParent.Child("poster.jpg")		      end if		      		    end if		    		  end if		  		  AnimeAttr.ReadInformation		  me.Refresh		End Sub	#tag EndEvent	#tag Event		Function DragOver(x As Integer, y As Integer, obj As DragItem, action As Integer) As Boolean		  Dim factor as Double		  Dim dropPic, pic as Picture		  Dim PicDimensions As String		  		  if obj <> nil then		    me.Graphics.ForeColor = &c626262		    me.Graphics.FillRect 0, 0, me.Width, me.Height		    me.Graphics.ForeColor = &c000000		    me.Graphics.DrawRect 0, 0, me.Width, me.Height		    		    dropPic = Picture.Open( obj.FolderItem )		    factor = Min( me.Width / dropPic.Width, me.Height / dropPic.Height )		    factor = Min( factor, 1.0 )		    pic = New Picture( dropPic.Width * factor, dropPic.Height * factor, 32 )		    pic.Graphics.DrawPicture( dropPic, 0, 0, pic.Width, pic.Height,  0, 0, dropPic.Width, dropPic.Height )		    		    me.Graphics.DrawPicture pic, (me.Width / 2) - (pic.Width / 2), (me.Height / 2) - (pic.Height / 2)		    		    PicDimensions = "Size: " + str( dropPic.Width ) + "x" + str(dropPic.Height)		    me.Graphics.TextSize = 11		    me.Graphics.Bold = True		    me.Graphics.ForeColor = &cFFFFFF		    me.Graphics.FillRect 3, 3, ( PicDimensions.Len * 7 ), me.Graphics.TextHeight + 10		    me.Graphics.ForeColor = &c000000		    me.Graphics.DrawRect 3, 3, ( PicDimensions.Len * 7 ), me.Graphics.TextHeight + 10		    me.Graphics.DrawString( PicDimensions, 9, 8 + me.Graphics.TextSize )		    		    me.Graphics.ForeColor = &c000000		    me.Graphics.DrawRect 0, 0, me.Width, me.Height		  end if		End Function	#tag EndEvent	#tag Event		Sub DragExit(obj As DragItem, action As Integer)		  me.Refresh(false)		End Sub	#tag EndEve