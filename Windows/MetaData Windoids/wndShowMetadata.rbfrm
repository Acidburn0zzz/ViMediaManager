eif Keyboard.AsyncCommandKey and Keyboard.AsyncKeyDown(&h15) then		    tpMetadata.Value = 3 // Poster		  elseif Keyboard.AsyncCommandKey and Keyboard.AsyncKeyDown(&h16) then		    tpMetadata.Value = 5 // Fanart		  elseif Keyboard.AsyncCommandKey and Keyboard.AsyncKeyDown(&h17) then		    tpMetadata.Value = 4 // Banner		  elseif Keyboard.AsyncCommandKey and Keyboard.AsyncKeyDown(&h1A) then		    tpMetadata.Value = 6 // ExtraArt		  end if		End Function	#tag EndEvent	#tag Event		Sub Open()		  'self.FullScreenAuxiliaryMBS = true		  Closed = False		  DistributeData		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub DistributeData()		  Dim Hours, Minutes, i As Integer		  Dim sGenre As String		  		  if TVAttr.Title <> "" then		    self.Title = TVAttr.Title		  else		    self.Title = wndMain.lstTVShows.cell( wndMain.lstTVShows.ListIndex, 0 )		  end if		  		  // Show Info		  edtTitle.Text = TVAttr.Title		  edtTitleOriginal.Text = TVAttr.OriginalTitle		  edtTitleSort.Text = TVAttr.SortTitle		  edtPlot.Text = TVAttr.Plot		  edtReleaseDate.Text = TVAttr.Premiered		  edtYear.Text = TVAttr.Year		  if TVAttr.Rating < 0 then edtRating.Text = "0" else edtRating.Text = str( TVAttr.Rating )		  edtStudio.Text = TVAttr.Studio		  		  lblNextAiredValue.Text = TVAttr.NextAired		  lblNextEpisodeValue.Text = TVAttr.NextEpisode		  		  // MPAA Rating		  select case TVAttr.MPAA		  case ""		    popMPAARating.ListIndex = 0		  case "TV-G"		    popMPAARating.ListIndex = 1		  case "TV-PG"		    popMPAARating.ListIndex = 2		  case "TV-14"		    popMPAARating.ListIndex = 3		  case "TV-MA"		    popMPAARating.ListIndex = 4		  case "TV-Y"		    popMPAARating.ListIndex = 5		  case "TV-Y7"		    popMPAARating.ListIndex = 6		  case "TV-Y7FV"		    popMPAARating.ListIndex = 7		  end select		  		  // File Path Label		  lblSummaryFilePath.Text = TVAttr.FolderParent.AbsolutePath.ReplaceAll("&", "&&")		  		  // IMDb & TMDb ID		  lblTVDBValue.Text = TVAttr.TVDBID		  lblTVDBValue.URL = "http://thetvdb.com/?tab=series&id=" + TVAttr.TVDBID		  		  lblIMDBValue.Text = TVAttr.IMDBID		  lblIMDBValue.URL = "http://www.imdb.com/title/" + TVAttr.IMDBID		  		  // Runtime Label		  lblSummaryTitle.Text = TVAttr.Title.ReplaceAll("&", "&&")		  if TVAttr.RuntimeInMinutes < 61 then		    lblRuntimeValue.Text = str(TVAttr.RuntimeInMinutes) + " Minutes"		  else		    Hours = floor( TVAttr.RuntimeInMinutes / 60 )		    Minutes = TVAttr.RuntimeInMinutes - (60 * Hours)		    if hours < 2 and Minutes > 0 then		      lblRuntimeValue.Text = str(hours) + " Hour and " + str(Minutes) + " Minutes"		    elseif hours < 2 then		      lblRuntimeValue.Text = str(hours) + " Hour"		    elseif Minutes > 0 then		      lblRuntimeValue.Text = str(hours) + " Hours and " + str(Minutes) + " Minutes"		    else		      lblRuntimeValue.Text = str(hours) + " Hours"		    end if		  end if		  		  // Actor List		  lstActors.DeleteAllRows		  for i = 0 to TVAttr.ActorName.Ubound		    lstActors.AddRow TVAttr.ActorName(i), TVAttr.ActorRole(i), TVAttr.ActorThumbURL(i)		  next		  		  // Genres		  for i = 0 to TVAttr.Genre.Ubound		    		    if i = 0 then		      sGenre = TVAttr.Genre(i)		    elseif i = TVAttr.Genre.Ubound then		      sGenre = sGenre + ", " + TVAttr.Genre(i)		    else		      sGenre = sGenre + ", " + TVAttr.Genre(i)		    end if		    		  next		  		  edtGenres.Text = sGenre		  		  // Prev & Next Buttons		  if wndMain.ppMedia.Value = 1 and wndMain.lstTVShows.ListIndex = wndMain.lstTVShows.ListCount - 1 then btnNext.Enabled = False else btnNext.Enabled = True		  if wndMain.ppMedia.Value = 1 and wndMain.lstTVShows.ListIndex = 0 then btnPrev.Enabled = False else btnPrev.Enabled = True		  		  if wndMain.ppMedia.Value = 2 and wndMain.lstAnime.ListIndex = wndMain.lstAnime.ListCount - 1 then btnNext.Enabled = False else btnNext.Enabled = True		  if wndMain.ppMedia.Value = 2 and wndMain.lstAnime.ListIndex = 0 then btnPrev.Enabled = False else btnPrev.Enabled = True		  		  // Canvas'		  cvsActorThumb.Refresh(false)		  cvsBannerFrame.Refresh(False)		  cvsExtras.Refresh(false)		  cvsFanartFrame.Refresh(false)		  cvsPosterFrame.Refresh(false)		  cvsSummaryBanner.Refresh(false)		  cvsSummaryPoster.Refresh(false)		  		  // TV Rage - Next Aired data		  pgWheelNextAired.Visible = True		  Scrape.TVRageNextAired( String2Entities( TVAttr.OriginalTitle.ReplaceAll("&", "and") ) )		  if NOT Closed then		    lblNextAiredValue.Text = TVAttr.NextAired		    lblNextAiredValue.HelpTag = TVAttr.NextAired		    lblNextEpisodeValue.Text = TVAttr.NextEpisode		    lblNextEpisodeValue.HelpTag = TVAttr.NextEpisode		    pgWheelNextAired.Visible = False		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub SaveData()		  Dim sGenres As String		  Dim i As Integer		  		  // Show Info		  TVAttr.Title = edtTitle.Text		  TVAttr.OriginalTitle = edtTitleOriginal.Text		  TVAttr.SortTitle = edtTitleSort.Text		  TVAttr.Plot = edtPlot.Text		  TVAttr.Premiered = edtReleaseDate.Text		  TVAttr.Year = edtYear.Text		  TVAttr.Rating = val( edtRating.Text )		  TVAttr.Studio = edtStudio.Text		  		  // MPAA Rating		  select case popMPAARating.ListIndex		  case 0		    TVAttr.MPAA = ""		  case 1		    TVAttr.MPAA = "TV-G"		  case 2		    TVAttr.MPAA = "TV-PG"		  case 3		    TVAttr.MPAA = "TV-14"		  case 4		    TVAttr.MPAA = "TV-MA"		  case 5		    TVAttr.MPAA = "TV-Y"		  case 6		    TVAttr.MPAA = "TV-Y7"		  case 7		    TVAttr.MPAA = "TV-Y7FV"		  end select		  		  // Actor List		  ReDim TVAttr.ActorName(-1)		  ReDim TVAttr.ActorRole(-1)		  ReDim TVAttr.ActorThumbURL(-1)		  For i = 0 to lstActors.ListCount - 1		    		    if lstActors.Cell( i, 0 ) <> "" then		      TVAttr.ActorName.Append lstActors.Cell( i, 0 )		    else		      TVAttr.ActorName.Append ""		    end if		    		    if lstActors.Cell( i, 1 ) <> "" then		      TVAttr.ActorRole.Append lstActors.Cell( i, 1 )		    else		      TVAttr.ActorRole.Append "" 		    end if		    		    if lstActors.Cell( i, 2 ) <> "" then		      TVAttr.ActorThumbURL.Append lstActors.Cell( i, 2 )		    else		      TVAttr.ActorThumbURL.Append ""		    end if		    		  next		  		  // Genres		  sGenres = edtGenres.Text.ReplaceAll(", ", ",")		  TVAttr.Genre = sGenres.Split(",")		  		  // Save Data		  TVAttr.WriteInformation( TVAttr.FolderParent )		End Sub	#tag EndMethod	#tag Property, Flags = &h1		Protected ActorIMG As Picture	#tag EndProperty	#tag Property, Flags = &h1		Protected ActorLastSelection As Integer = -1	#tag EndProperty	#tag Property, Flags = &h0		Closed As Boolean	#tag EndProperty#tag EndWindowCode#tag Events CancelSave	#tag Event		Sub ActionOK()		  SaveData		  		  if wndMain.ppMedia.Value = 1 then		    wndMain.PopulateTVShowsList		  elseif wndMain.ppMedia.Value = 2 then		    wndMain.PopulateAnimeList		  end if		  		  self.close		End Sub	#tag EndEvent	#tag Event		Sub Open()		  #if TargetMacOS then		    me.btnRight.Default = False		  #else		    me.btnLeft.Default = False		  #endif		End Sub	#tag EndEvent	#tag Event		Sub ActionCancel()		  		  		  if wndMain.ppMedia.Value = 1 then		    wndMain.PopulateTVShowsList		  elseif wndMain.ppMedia.Value = 2 then		    wndMain.PopulateAnimeList		  end if		  		  self.close		End Sub	#tag EndEvent#tag EndEvents#tag Events tpMetadata	#tag Event		Sub Open()		  me.Value = Preferences.ShowMetaDataWindowTab		End Sub	#tag EndEvent	#tag Event		Sub Change()		  Preferences.ShowMetaDataWindowTab = me.Value		  		  // #if TargetMacOS then self.SmoothResizeMBS( self.Width, WindowHeightDivider( me.Value ).top + 51 )		  #if TargetMacOS then SmoothResize( self, self.Width, WindowHeightDivider( me.Value ).Top + 51, 4 )		  self.Height = WindowHeightDivider( me.Value ).top + 52		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsSummaryPoster	#tag Event		Sub Paint(g As Graphics)		  g.ForeColor = &cFFFFFF		  g.FillRect 0, 0, me.Width, me.Height		  		  if TVAttr.ART_Poster <> nil then		    g.DrawPicture TVAttr.ART_Poster, 0, 0, me.Width, me.Height, 0, 0, TVAttr.ART_Poster.Width, TVAttr.ART_Poster.Height		  else		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Poster" + chr(13) + "Available", (me.Width / 2) - (g.StringWidth("No Poster" + chr(13) + "Available") / 4), (me.Height / 2) - (g.TextSize * 2), me.Width, False )		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent#tag EndEvents#tag Events lblSummaryTitle	#tag Event		Sub Open()		  if MovieAttr.Year <> "" then		    me.text = MovieAttr.Title + " (" + MovieAttr.Year + ")"		  else		    me.text = MovieAttr.Title		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsSummaryBanner	#tag Event		Sub Paint(g As Graphics)		  g.ForeColor = &cFFFFFF		  g.FillRect 0, 0, me.Width, me.Height		  		  if TVAttr.ART_Banner <> nil then		    g.DrawPicture TVAttr.ART_Banner, 0, 0, me.Width, me.Height, 0, 0, TVAttr.ART_Banner.Width, TVAttr.ART_Banner.Height		  else		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Banner" + chr(13) + "Available", (me.Width / 2) - (g.StringWidth("No Banner" + chr(13) + "Available") / 4), (me.Height / 2) - (g.TextSize * 2), me.Width, False )		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent#tag EndEvents#tag Events edtRating	#tag Event		Function KeyDown(Key As String) As Boolean		  		  if Keyboard.AsyncKeyDown(&h7E) or Keyboard.AsyncKeyDown(&h7C) then		    if Val( edtRating.Text ) < 10 then edtRating.Text = str( val( edtRating.Text ) + 0.1 )		  ElseIf Keyboard.AsyncKeyDown(&h7B) or Keyboard.AsyncKeyDown(&h7D) then		    if Val( edtRating.Text ) > 0 then edtRating.Text = str( val( edtRating.Text ) - 0.1 )		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events udRating	#tag Event		Sub Down()		  if Val( edtRating.Text ) > 0 then edtRating.Text = str( val( edtRating.Text ) - 0.1 )		End Sub	#tag EndEvent	#tag Event		Sub Up()		  if Val( edtRating.Text ) < 10 then edtRating.Text = str( val( edtRating.Text ) + 0.1 )		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsActorThumb	#tag Event		Sub Paint(g As Graphics)		  Dim pic as Picture		  		  g.ForeColor = &cFFFFFF		  g.FillRect 0, 0, me.Width, me.Height		  		  if ActorIMG <> nil then		    pic = ScalePic( ActorIMG, me.Width, me.Height, True, True, True )		    g.DrawPicture pic, 0, me.Height - pic.Height		  else		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Image" + chr(13) + "Available", (me.Width / 2) - (g.StringWidth("No Image" + chr(13) + "Available") / 4), (me.Height / 2) - (g.TextSize * 2), me.Width, False )		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent#tag EndEvents#tag Events ActorTimer	#tag Event		Sub Action()		  ActorSckt.Yield = true		  if lstActors.ListIndex > -1 then		    if lstActors.Cell( lstActors.ListIndex, 2 ).Left(4) = "http" then		      ActorIMG = Picture.FromData( ActorSckt.Get( lstActors.Cell( lstActors.ListIndex, 2 ), 30 ) )		      ActorSckt.close		    end if		    pgWheelActors.Visible = False		    cvsActorThumb.Refresh(false)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events lstActors	#tag Event		Sub Change()		  		  pgWheelActors.Visible = False		  		  if me.ListIndex > -1 and me.ListIndex <> ActorLastSelection then		    ActorIMG = nil		    cvsActorThumb.Refresh(false)		    		    if me.Cell(me.ListIndex, 2) <> ""  then		      pgWheelActors.Visible = true		      ActorTimer.Mode = 1		    end if		    		    ActorLastSelection = me.ListIndex		  end if		  		  if me.ListIndex > -1 then		    arActor.RemoveEnabled = True		    arActor.Refresh(false)		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  me.ColumnType(0) = me.TypeEditable		  me.ColumnType(1) = me.TypeEditable		  me.ColumnType(2) = me.TypeEditable		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  if Keyboard.AsyncKeyDown(&h33) or Keyboard.AsyncKeyDown(&h75) then		    if me.active and me.listindex <> -1 then		      me.RemoveRow( me.listindex )		    end if		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events arActor	#tag Event		Sub Open()		  me.RemoveEnabled = false		  me.Refresh(false)		End Sub	#tag EndEvent	#tag Event		Sub Remove()		  if lstActors.ListIndex > -1 then lstActors.RemoveRow lstActors.ListIndex		End Sub	#tag EndEvent	#tag Event		Sub Activate()		  me.Refresh(false)		End Sub	#tag EndEvent	#tag Event		Sub Add()		  lstActors.AddRow edtActorName.text, edtActorRole.Text, edtActorThumbURL.Text		  lstActors.ListIndex = lstActors.LastIndex		  		  edtActorName.Text     = ""		  edtActorRole.Text     = ""		  edtActorThumbURL.Text = ""		End Sub	#tag EndEvent#tag EndEvents#tag Events edtActorName	#tag Event		Function KeyDown(Key As String) As Boolean		  if Keyboard.AsyncKeyDown(&h4C) or Keyboard.AsyncKeyDown(&h24) then		    if me.Active and me.text <> "" then		      lstActors.AddRow edtActorName.Text, edtActorRole.Text, edtActorThumbURL.Text		      edtActorName.Text = ""		      edtActorRole.Text = ""		      edtActorThumbURL.Text = ""		    end if		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events edtActorRole	#tag Event		Function KeyDown(Key As String) As Boolean		  if Keyboard.AsyncKeyDown(&h4C) or Keyboard.AsyncKeyDown(&h24) then		    if me.Active and me.text <> "" then		      lstActors.AddRow edtActorName.Text, edtActorRole.Text, edtActorThumbURL.Text		      edtActorName.Text = ""		      edtActorRole.Text = ""		      edtActorThumbURL.Text = ""		    end if		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events edtActorThumbURL	#tag Event		Function KeyDown(Key As String) As Boolean		  if Keyboard.AsyncKeyDown(&h4C) or Keyboard.AsyncKeyDown(&h24) then		    if me.Active and me.text <> "" then		      lstActors.AddRow edtActorName.Text, edtActorRole.Text, edtActorThumbURL.Text		      edtActorName.Text = ""		      edtActorRole.Text = ""		      edtActorThumbURL.Text = ""		    end if		  end if		End Function	#tag EndEvent#tag EndEvents#tag Events bvlDeletePoster	#tag Event		Sub Action()		  Dim TheFile as FolderItem = TVAttr.ShowPosterFilter		  		  if TheFile <> nil and TheFile.Exists then		    if dlgWindow("Are you sure you want to delete the current poster?", "This cannot be undone") then		      TheFile.Delete		      TVAttr.ART_Poster = nil		      cvsPosterFrame.Refresh(false)		    end if		  Else		    MsgBox "No poster to delete"		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsPosterFrame	#tag Event		Sub Paint(g As Graphics)		  Dim pic as Picture		  Dim PicDimensions As String		  		  g.ForeColor = &c929292		  g.FillRect 0, 0, me.Width, me.Height		  		  if TVAttr.ART_Poster <> Nil then		    pic = ScalePic( TVAttr.ART_Poster, me.Width, me.Height )		    g.DrawPicture pic, (me.Width / 2) - (pic.Width / 2), (me.Height / 2) - (pic.Height / 2)		    		    // Sizes label ---------		    PicDimensions = "Size: " + str(TVAttr.ART_Poster.Width) + "x" + str(TVAttr.ART_Poster.Height)		    		    g.TextSize = 11		    g.Bold = True		    		    g.ForeColor = &CFFFFFF		    g.FillRect 3, 3, (PicDimensions.Len * 7), g.TextHeight + 10		    g.ForeColor = &c000000		    g.DrawRect 3, 3, (PicDimensions.Len * 7), g.TextHeight + 10		    g.DrawString( PicDimensions, 9, 8 + g.TextSize )		    		    pic = Nil		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent	#tag Event		Sub Open()		  		  me.AcceptFileDrop("image/png")		  me.AcceptFileDrop("image/jpg")		End Sub	#tag EndEvent	#tag Event		Sub DropObject(obj As DragItem, action As Integer)		  Dim PosterPath As FolderItem		  Dim Message, Explanation, PosterNam