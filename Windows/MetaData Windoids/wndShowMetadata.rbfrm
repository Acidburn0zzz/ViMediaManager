Item.CopyFileTo TVAttr.FolderParent.Child("Landscape.jpg")		      end select		      		    end if		    		  end if		  		  TVAttr.ReadInformation		  me.Refresh		End Sub	#tag EndEvent#tag EndEvents#tag Events popArt	#tag Event		Sub Change()		  cvsExtras.Refresh(false)		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlDownload	#tag Event		Sub Action()		  select case popArt.ListIndex		  case 0		    Scrape.FanartTVApi( TVAttr.TVDBID, "clearart" )		  case 1		    Scrape.FanartTVApi( TVAttr.TVDBID, "clearlogo" )		  case 2		    Scrape.FanartTVApi( TVAttr.TVDBID, "tvthumb" )		  end select		  		  		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlDVDIcon	#tag Event		Sub Action()		  if SetDVDIcon( TVAttr.ART_Poster, TVAttr.FolderParent ) = 0 then		    MsgBox "Folder icon has been set" + chr(13) + chr(13) + "You may need to refresh the finder by logging out and back in for the icon to show up."		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  #if NOT TargetMacOS then me.Visible = False		  me.Visible = DebugBuild		End Sub	#tag EndEvent#tag EndEvents#tag Events btnPrev	#tag Event		Sub Action()		  ActorSckt.Close		  CleanTemporaryFiles		  		  if wndMain.ppMedia.Value = 1 and wndMain.lstTVShows.ListIndex > 0 then		    'if wndMain.lstTVShows.Cell( wndMain.lstTVShows.ListIndex, 1 ) <> "" then TVAttr.WriteInformation( TVAttr.FolderParent.Child( "tvshow.nfo" ) )		    SaveData		    wndMain.lstTVShows.ListIndex = wndMain.lstTVShows.ListIndex - 1		    		  elseif wndMain.ppMedia.Value = 2 and wndMain.lstAnime.ListIndex > 0 then		    'if wndMain.lstAnime.Cell( wndMain.lstAnime.ListIndex, 1 ) <> "" then TVAttr.WriteInformation( TVAttr.FolderParent.Child( "tvshow.nfo" ) )		    SaveData		    wndMain.lstAnime.ListIndex = wndMain.lstAnime.ListIndex - 1		    		  end if		  		  DistributeData		End Sub	#tag EndEvent#tag EndEvents#tag Events btnNext	#tag Event		Sub Action()		  ActorSckt.Close		  CleanTemporaryFiles	