ions As String		  		  if obj <> nil then		    me.Graphics.ForeColor = &c626262		    me.Graphics.FillRect 0, 0, me.Width, me.Height		    me.Graphics.ForeColor = &c000000		    me.Graphics.DrawRect 0, 0, me.Width, me.Height		    		    dropPic = Picture.Open( obj.FolderItem )		    		    if dropPic.Width = 758 and dropPic.Height = 140 then		      factor = Min( me.Width / dropPic.Width, me.Height / dropPic.Height )		      factor = Min( factor, 1.0 )		      pic = New Picture( dropPic.Width * factor, dropPic.Height * factor, 32 )		      pic.Graphics.DrawPicture( dropPic, 0, 0, pic.Width, pic.Height,  0, 0, dropPic.Width, dropPic.Height )		      		      me.Graphics.DrawPicture pic, (me.Width / 2) - (pic.Width / 2), (me.Height / 2) - (pic.Height / 2)		      		      PicDimensions = "Size: " + str( dropPic.Width ) + "x" + str(dropPic.Height)		      me.Graphics.TextSize = 11		      me.Graphics.Bold = True		      me.Graphics.ForeColor = &cFFFFFF		      me.Graphics.FillRect 3, 3, ( PicDimensions.Len * 7 ), me.Graphics.TextHeight + 10		      me.Graphics.ForeColor = &c000000		      me.Graphics.DrawRect 3, 3, ( PicDimensions.Len * 7 ), me.Graphics.TextHeight + 10		      me.Graphics.DrawString( PicDimensions, 9, 8 + me.Graphics.TextSize )		    elseif obj.FolderItem.Type <> "image/jpg" then		      me.Graphics.ForeColor = &cFFFFFF		      me.Graphics.TextSize = 18		      me.Graphics.DrawString( "Invalid filetype, convert to JPG", (me.Width / 2) - (me.Graphics.StringWidth("Invalid filetype, convert to JPG") / 2), (me.Height / 2) - (me.Graphics.TextSize * 2), me.Width, False )		      		    elseif dropPic.Width <> 758 and dropPic.Height <> 140 then		      me.Graphics.ForeColor = &cFFFFFF		      me.Graphics.TextSize = 18		      me.Graphics.DrawString( "Invalid banner size, resize to 758x140", (me.Width / 2) - (me.Graphics.StringWidth("Invalid bannersize, resize to 758x140") / 2), (me.Height / 2) - (me.Graphics.TextSize * 2), me.Width, False )		    end if		