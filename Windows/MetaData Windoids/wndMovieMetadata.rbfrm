s DragItem, action As Integer)		  #pragma disablebackgroundtasks		  #if not targetwin32 then		    #pragma disableboundsChecking		    #pragma nilObjectChecking false		    #pragma stackOverflowChecking false		    #pragma disableautoWaitCursor		  #endif		  		  Dim FanartPath As FolderItem		  Dim Message, Explanation, FanartName as String		  		  Explanation = "If you choose 'OK' the existing image will be replaced with the new image."		  Message = "Poster image already exists, replace image?"		  		  FanartPath = MovieAttr.MovieFanartFilter( MovieAttr.FolderParent )		  		  if obj.FolderItemAvailable and obj.FolderItem <> nil then		    		    if FanartPath <> nil and FanartPath.Exists then		      		      FanartName = FanartPath.Name.Left( FanartPath.Name.Len - 4 )		      		      if dlgWindow(Message, Explanation) then		        FanartPath.Delete		        if obj.FolderItem.Type = "image/png" then		          obj.FolderItem.CopyFileTo MovieAttr.FolderParent.Child( FanartName + ".png")		        elseif obj.FolderItem.Type = "image/jpg" then		          obj.FolderItem.CopyFileTo MovieAttr.FolderParent.Child( FanartName + ".jpg")		        end if		      end if		      		    else		      		      if obj.FolderItem.Type = "image/png" then		        obj.FolderItem.CopyFileTo MovieAttr.FolderParent.Child("fanart.png")		      elseif obj.FolderItem.Type = "image/jpg" then		        obj.FolderItem.CopyFileTo MovieAttr.FolderParent.Child("fanart.jpg")		      end if		      		    end if		    		  end if		  		  MovieAttr.ReadInformation		  me.Refresh		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlDownloadFanart	#tag Event		Sub Action()		  if MovieAttr.TMDbID <> "" then		    Scrape.TMDBApiArt( MovieAttr.TMDbID, "Fanart" )		  elseif MovieAttr.IMDBID <> "" then		    Scrape.TMDBApiArt( MovieAttr.IMDBID, "Fanart" )		  else		    MsgBox "I don't know which movie this is, scrape some information first!"		  end if		  cvsFanartFrame.Refresh(False)		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlDeletePoster	#tag Event		Sub Action()		  Dim TheFile as FolderItem = MovieAttr.MoviePosterFilter( MovieAttr.FolderParent )		  		  if TheFile <> nil and TheFile.Exists then		    if dlgWindow("Are you sure you want to delete the current poster?", "This cannot be undone") then		      TheFile.Delete		      MovieAttr.ART_Poster = nil		      cvsPosterFrame.Refresh(false)		    end if		  Else		    MsgBox "No poster to delete"		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlDeleteFanart	#tag Event		Sub Action()		  Dim TheFile as FolderItem = MovieAttr.MovieFanartFilter( MovieAttr.FolderParent )		  		  if TheFile <> nil and TheFile.Exists then		    if dlgWindow("Are you sure you want to delete the current fanart?", "This cannot be undone") then		      TheFile.Delete		      MovieAttr.ART_Fanart = nil		      cvsFanartFrame.Refresh(false)		    end if		  else		    MsgBox "No fanart to delete"		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events TrailerSocket	#tag Event		Sub PageReceived(url as string, httpStatus as integer, headers as internetHeaders, content as string)		  Dim tmpContent As String = content		  'TextArea1.text = tmpContent + chr(13) + "--------------------" + chr(13) + TextArea1.Text		  		  if tmpContent.Left(5) = "<?xml" then		    GetYouTubeURL( tmpContent )		  else		    dim idx1, idx2 As integer		    		    idx1 = tmpContent.InStr("img.src = """) + 11  //extract the 'generate' url		    idx2 = tmpContent.InStr(idx1, ";")		    dim s As String = tmpContent.Mid(idx1, idx2 - idx1 - 1)		    'Dim MovieName As String = MovieNameFilter( MovieFolder )		    		    s = s.ReplaceAll("\/", "/")         //unencode these characters		    s = s.ReplaceAll("\u0026", "&")		    		    s = s.Replace("generate_204", "videoplayback")   //switch generate to video		    s = s.Replace("pmbypass&", "")         //remove these bypass things		    s = s.Replace("pmbypass=yes&", "")		    		    //download the movie to a file		    me.Get s, SpecialFolder.Temporary.Child( "trailer.downloading" )		    		  end if		End Sub	#tag EndEvent	#tag Event		Sub ReceiveProgress(bytesReceived as integer, totalBytes as integer, newData as string)		  pgBar.Maximum = totalBytes		  pgBar.Value = bytesReceived		End Sub	#tag EndEvent	#tag Event		Sub DownloadComplete(url as string, httpStatus as integer, headers as internetHeaders, file as folderItem)		  Dim MovieName As String = MovieAttr.MovieNameFilter( AnimeAttr.FolderParent, False )		  		  if file <> nil and file.Exists and file.PhysicalFileDataLengthMBS > 100 then		    		    if MovieAttr.MovieTrailerFilter( AnimeAttr.FolderParent ) <> nil and MovieAttr.MovieTrailerFilter( AnimeAttr.FolderParent ).Exists then MovieAttr.MovieTrailerFilter( AnimeAttr.FolderParent ).Delete		    		    if MovieName <> "" then		      file.MoveFileTo( AnimeAttr.FolderParent.Child( MovieName + "-trailer.flv") )		    else		      file.MoveFileTo( AnimeAttr.FolderParent.Child( "trailer.flv" ) )		    end if		  else		    MsgBox str(me.LastErrorCode) + " Cannot open the file?"		  end if		  		  me.Close		  		  if MovieAttr.MovieTrailerFilter( AnimeAttr.FolderParent ) <> nil and MovieAttr.MovieTrailerFilter( AnimeAttr.FolderParent ).Exists then		    lblTrailerAvailable.Text = "Available"		    bvlDeleteTrailer.Enabled = True		  else		    lblTrailerAvailable.Text = "N/A"		    bvlDeleteTrailer.Enabled = False		  end if		  		  pgBar.Visible = False		End Sub	#tag EndEvent	#tag Event		Sub Error(code as integer)		  'MsgBox "Error: " + str(code)		  SocketError( code, me )		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlTrailerHD	#tag Event		Sub Action()		  QualityHD = True		  		  pgBar.Visible = True		  		  if MovieAttr.TMDbID <> "" then		    TrailerSocket.Get( "http://api.themoviedb.org/2.1/Movie.getInfo/" + DBLanguage + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.TMDbID )		  else		    TrailerSocket.Get( "http://api.themoviedb.org/2.1/Movie.imdbLookup/" + DBLanguage + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.IMDBID )		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlTrailerSD	#tag Event		Sub Action()		  QualityHD = False		  		  pgBar.Visible = True		  		  if MovieAttr.TMDbID <> "" then		    TrailerSocket.Get( "http://api.themoviedb.org/2.1/Movie.getInfo/" + DBLanguage + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.TMDbID )		  else		    TrailerSocket.Get( "http://api.themoviedb.org/2.1/Movie.imdbLookup/" + DBLanguage + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.IMDBID )		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events pgBar	#tag Event		Sub Close()		  me.Visible = False		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlDeleteTrailer	#tag Event		Sub Action()		  if MovieAttr.MovieAttr.MovieTrailerFilter( MovieAttr.FolderParent ) <> nil and MovieAttr.MovieTrailerFilter( MovieAttr.FolderParent ).Exists then		    		    if dlgWindow("Are you sure you want to delete the current trailer?", "This cannot be undone") then		      MovieAttr.MovieAttr.MovieTrailerFilter( MovieAttr.FolderParent ).Delete		    end if		    		  else		    MsgBox "No trailer to delete"		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  if MovieAttr.MovieTrailerFilter( MovieAttr.FolderParent ) <> nil and MovieAttr.MovieTrailerFilter( MovieAttr.FolderParent ).Exists then		    me.Enabled = true		  else		    me.Enabled = False		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events UDaRating	#tag Event		Sub Down()		  if Val( edtRating.Text ) > 0 then edtRating.Text = str( val( edtRating.Text ) - 0.1 )		End Sub	#tag EndEvent	#tag Event		Sub Up()		  if Val( edtRating.Text ) < 10 then edtRating.Text = str( val( edtRating.Text ) + 0.1 )		End Sub	#tag EndEvent#tag EndEvents#tag Events bvlSetIcon	#tag Event		Sub Action()		  if SetDVDIcon( MovieAttr.ART_Poster, MovieAttr.FolderParent ) = 0 then		    MsgBox "Folder icon has been set" + chr(13) + chr(13) + "You may need to refresh the finder by logging out and back in for the icon to show up."		  end if		End Sub	#tag EndEvent	#tag Event		Sub Open()		  #if NOT TargetMacOS then me.Visible = False		  me.Visible = DebugBuild		End Sub	#tag EndEvent#tag EndEvents#tag Events btnNext	#tag Event		Sub Action()		  if wndMain.lstMovies.ListIndex < wndMain.lstMovies.ListCount - 1 then		    ActorSckt.Close		    TrailerSocket.Close		    CleanTemporaryFiles		    		    if wndMain.lstMovies.Cell( wndMain.lstMovies.ListIndex, 1 ) <> "" then MovieAttr.WriteInformation( MovieAttr.FolderParent )		    wndMain.lstMovies.ListIndex = wndMain.lstMovies.ListIndex + 1		    SetInfo		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events btnPrev	#tag Event		Sub Action()		  if wndMain.lstMovies.ListIndex > 0 then		    ActorSckt.Close		    TrailerSocket.Close		    CleanTemporaryFiles		    		    if wndMain.lstMovies.Cell( wndMain.lstMovies.ListIndex, 1 ) <> "" then MovieAttr.WriteInformation( MovieAttr.FolderParent )		    wndMain.lstMovies.ListIndex = wndMain.lstMovies.ListIndex - 1		    SetInfo		  end if		End Sub	#tag EndEvent#tag EndEvents