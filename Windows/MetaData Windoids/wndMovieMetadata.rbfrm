syncKeyDown(&h17) then		    tpMetadata.Value = 4 // Trailer		  end if		End Function	#tag EndEvent	#tag Event		Sub Open()		  'self.SetWindowProxyIconMBS("APPL", "ViMM")		  AddRemoveActorButtons.RemoveEnabled = false		  AddRemoveStudios.RemoveEnabled = false		  		  'self.FullScreenAuxiliaryMBS = true		  		  SetInfo		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub GetYouTubeURL(XmlContent As String)		  Dim Xml as new XmlDocument		  Dim xroot, xnode as XmlNode		  Dim i as Integer		  Dim s as String		  		  Xml.PreserveWhitespace = false		  		  'if XmlContent.Left(5) = "<?xml" then		  Xml.LoadXml( XmlContent )		  		  xroot = Xml.Child(0).Child(2).FirstChild		  		  for i = 0 to xroot.ChildCount - 1		    		    xnode = xroot.Child(i)		    		    if xnode.FirstChild <> nil then		      		      if MovieAttr.TMDbID = "" and xnode.Name = "id" then		        MovieAttr.TMDbID = xnode.FirstChild.Value		        s = MovieAttr.TMDbID		        TrailerSocket.Get( "http://api.themoviedb.org/2.1/Movie.getInfo/" + DBLanguage + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.TMDbID )		        		      elseif MovieAttr.TMDbID <> "" and xnode.Name = "Trailer" then		        s = xnode.FirstChild.Value		        		        if QualityHD and s <> "" then		          TrailerSocket.Get( s + "&hd=1" )		        elseif QualityHD = False and s <> "" then		          TrailerSocket.Get( s + "&hd=0" )		        elseif s = "" then		          MsgBox "No trailer available"		          pgBar.Visible = False		        end if		        		      end if		      		    end if		    		  next		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub SaveNewData()		  ReDim MovieAttr.Genre(-1)		  ReDim MovieAttr.Studios(-1)		  		  ReDim MovieAttr.ActorName(-1)		  ReDim MovieAttr.ActorRole(-1)		  ReDim MovieAttr.ActorThumbURL(-1)		  		  Dim i As Integer		  Dim s As String		  		  MovieAttr.Title = edtTitle.Text		  MovieAttr.SortTitle = edtSortTitle.Text		  MovieAttr.OriginalTitle = edtOriginalTitle.Text		  		  MovieAttr.Tagline = edtTagline.Text		  MovieAttr.Outline = edtOutline.Text		  MovieAttr.Plot = edtPlot.Text		  		  MovieAttr.Rating = val( edtRating.Text )		  MovieAttr.MPAA = popMPAA.Text		  		  MovieAttr.Premiered = edtPremiered.Text		  MovieAttr.Year = edtYear.Text		  		  if lstInfoStudios.ListCount > 0 then MovieAttr.Studio = lstInfoStudios.Cell( 0, 0 )		  		  for i = 0 to lstInfoStudios.ListCount - 1		    if lstInfoStudios.Cell( i, 0 ) <> "" then MovieAttr.Studios.Append lstInfoStudios.Cell( i, 0 )		  next		  		  For i = 0 to lstActors.ListCount - 1		    		    if lstActors.Cell( i, 0 ) <> "" then		      MovieAttr.ActorName.Append lstActors.Cell( i, 0 )		    else		      MovieAttr.ActorName.Append ""		    end if		    		    if lstActors.Cell( i, 1 ) <> "" then		      MovieAttr.ActorRole.Append lstActors.Cell( i, 1 )		    else		      MovieAttr.ActorRole.Append ""		    end if		    		    if lstActors.Cell( i, 2 ) <> "" then		      MovieAttr.ActorThumbURL.Append lstActors.Cell( i, 2 )		    else		      MovieAttr.ActorThumbURL.Append ""		    end if		    		  next		  		  s = edtGenres.Text.ReplaceAll(", ", ",")		  MovieAttr.Genre = s.Split(",")		  		  MovieAttr.Director = edtDirector.Text		  MovieAttr.Writer = edtWriter.Text		  		  if MovieAttr.MovieNFOFilter( MovieAttr.FolderParent ) <> nil then		    MovieAttr.WriteInformation( MovieAttr.MovieNFOFilter( MovieAttr.FolderParent ) )		  else		    MovieAttr.WriteInformation( MovieAttr.FolderParent.Child("movie.nfo") )		  end if		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub SetInfo()		  Dim GB, MB, bytes as Double		  Dim i, Hours, Minutes as Integer		  Dim sGenres As String		  Dim MovieFile As FolderItem = MovieAttr.MovieItemFilter( MovieAttr.FolderParent)		  		  if MovieAttr.Year <> "" then		    me.Title = MovieAttr.Title + " (" + MovieAttr.Year + ")"		  else		    me.Title = MovieAttr.Title		  end if		  		  'MovieFolder = wndMain.lstMovies.CellTag( wndMain.lstMovies.ListIndex, 0 )		  		  // Information Summary		  // File size		  if MovieFile <> nil and MovieFile.Exists then		    bytes = MovieFile.PhysicalFileTotalLengthMBS		    		    #if TargetMacOS then		      // Mac's use 1000's for bytecount.		      MB = bytes / 1000 / 1000		      GB = MB / 1000		      		      // Return ###.## MB		      MB = Floor( MB * 100 ) / 100		      GB = Floor( GB * 100 ) / 100		    #else		      // Windows still uses 1024's for bytecount.		      MB = bytes / 1024 / 1024		      GB = MB / 1024		      		      // Return ###.## MB		      MB = Floor( MB * 100 ) / 100		      GB = Floor( GB * 100 ) / 100		    #endif		    if MB > 1000 then lblSizeValue.text = str(GB) + " GB" else lblSizeValue.text = str(MB) + " MB"		    		    lblSummaryFilePath.text = MovieFile.AbsolutePath		  else		    lblSizeValue.Text = ""		    lblSummaryFilePath.Text = "No movie found"		  end if // @END f exists		  		  // Video Dimensions		  if MovieAttr.videoinfoHeight > -1 then		    lblDimensionsValue.Text = Str( MovieAttr.videoinfoWidth ) + "w x " + Str( MovieAttr.videoinfoHeight ) + "h"		  else		    lblDimensionsValue.Text = ""		  end if		  		  // Runtime		  if MovieAttr.videoinfoDurationInMinutes <> "" then		    		    hours = floor( val(MovieAttr.videoinfoDurationInMinutes) / 60 )		    		    if val( MovieAttr.videoinfoDurationInMinutes ) > 60 and val( MovieAttr.videoinfoDurationInMinutes ) < 120 then		      minutes = val(MovieAttr.videoinfoDurationInMinutes) - ( hours * 60 )		      lblRuntimeValue.text = str(hours) + " hour and " + str(minutes) + " minutes"		    elseif val( MovieAttr.videoinfoDurationInMinutes ) > 120 then		      minutes = val(MovieAttr.videoinfoDurationInMinutes) - ( hours * 60 )		      lblRuntimeValue.text = str(hours) + " hours and " + str(minutes) + " minutes"		    else		      lblRuntimeValue.text = MovieAttr.videoinfoDurationInMinutes + " minutes"		    end if		    		  else		    lblRuntimeValue.Text = ""		  end if		  		  // Video Aspect		  if MovieAttr.videoinfoAspect > -1 then		    lblAspectRatioValue.text = str(MovieAttr.videoinfoAspect) + ":1"		  else		    lblAspectRatioValue.Text = ""		  end if		  		  // IMDb & TMDb ID		  lblTMDBValue.Text = MovieAttr.TMDbID		  lblTMDBValue.URL = "http://www.themoviedb.org/movie/" + MovieAttr.TMDbID		  		  lblIMDBValue.Text = MovieAttr.IMDBID		  lblIMDBValue.URL = "http://www.imdb.com/title/" + MovieAttr.IMDBID		  		  // Information Metadata		  // Simple		  edtTitle.Text = MovieAttr.Title		  lblTitle.Text = MovieAttr.Title		  edtOriginalTitle.Text = MovieAttr.OriginalTitle		  edtSortTitle.Text = MovieAttr.SortTitle		  		  edtTagline.Text = MovieAttr.Tagline		  edtOutline.Text = MovieAttr.Outline		  lblSummaryOutline.Text = MovieAttr.Outline		  edtPlot.Text    = MovieAttr.Plot		  		  if MovieAttr.Rating > -1 then edtRating.Text = str( MovieAttr.Rating ) else edtRating.Text = "0"		  edtPremiered.Text = MovieAttr.Premiered		  edtYear.Text = MovieAttr.Year		  		  // Special		  // MPAA popdown menu		  select case MovieAttr.MPAA		  case ""		    popMPAA.ListIndex = 0		  case "G"		    popMPAA.ListIndex = 1		  case "PG"		    popMPAA.ListIndex = 2		  case "PG-13"		    popMPAA.ListIndex = 3		  case "NC-17"		    popMPAA.ListIndex = 4		  case "R"		    popMPAA.ListIndex = 5		  end select		  		  // Movie Studios List		  lstInfoStudios.DeleteAllRows		  for i = 0 to MovieAttr.Studios.Ubound		    lstInfoStudios.AddRow MovieAttr.Studios(i)		  next		  		  // Genres		  for i = 0 to MovieAttr.Genre.Ubound		    if i = 0 then		      sGenres = MovieAttr.Genre(i)		    elseif i = MovieAttr.Genre.Ubound then		      sGenres = sGenres + ", " + MovieAttr.Genre(i)		    else		      sGenres = sGenres + ", " + MovieAttr.Genre(i)		    end if		  next		  edtGenres.Text = sGenres		  		  // People		  // Actors		  lstActors.DeleteAllRows		  for i = 0 to MovieAttr.ActorName.Ubound		    lstActors.AddRow MovieAttr.ActorName(i), MovieAttr.ActorRole(i), MovieAttr.ActorThumbURL(i)		  next		  		  // Credits		  edtDirector.Text = MovieAttr.Director		  edtWriter.Text   = MovieAttr.Writer		  		  // Trailer		  if MovieAttr.MovieTrailerFilter(MovieAttr.FolderParent) <> nil and MovieAttr.MovieTrailerFilter(MovieAttr.FolderParent).Exists then		    lblTrailerAvailable.Text = "Available"		  else		    lblTrailerAvailable.Text = "N/A"		  end if		  		  // Canvas'		  cvsActorThumb.Refresh(false)		  cvsFanartFrame.Refresh(false)		  cvsPosterFrame.Refresh(false)		  cvsSummaryPoster.Refresh(false)		  		  // Next/Prev Buttons		  if wndMain.lstMovies.ListIndex = wndMain.lstMovies.ListCount - 1 then btnNext.Enabled = False else btnNext.Enabled = True		  if wndMain.lstMovies.ListIndex = 0 then btnPrev.Enabled = False else btnPrev.Enabled = True		End Sub	#tag EndMethod	#tag Property, Flags = &h0		ActorIMG As Picture	#tag EndProperty	#tag Property, Flags = &h0		ActorLastSelection As Integer = -1	#tag EndProperty	#tag Property, Flags = &h0		MetaStudios() As String	#tag EndProperty	#tag Property, Flags = &h0		QualityHD As Boolean = True	#tag EndProperty#tag EndWindowCode#tag Events CancelSave	#tag Event		Sub ActionCancel()		  		  		  ActorTimer.Mode = 0		  ActorSckt.close		  TrailerSocket.Close		  CleanTemporaryFiles		  self.close		End Sub	#tag EndEvent	#tag Event		Sub ActionOK()		  SaveNewData		  		  ActorTimer.Mode = 0		  ActorSckt.close		  TrailerSocket.Close		  CleanTemporaryFiles		  self.close		  		  wndMain.PopulateMoviesList		End Sub	#tag EndEvent	#tag Event		Sub Open()		  #if TargetMacOS then		    me.btnRight.Default = false		  #else		    me.btnLeft.Default = False		  #endif		End Sub	#tag EndEvent#tag EndEvents#tag Events tpMetadata	#tag Event		Sub Change()		  Preferences.MovieMetaDataWindowTab = me.Value		  		  #if TargetMacOS then self.SmoothR