nt(g As Graphics)		  Dim pic as Picture		  		  g.ForeColor = &cFFFFFF		  g.FillRect 0, 0, me.Width, me.Height		  		  if ActorIMG <> nil then		    pic = ScalePic( ActorIMG, me.Width, me.Height, True, True, True )		    g.DrawPicture pic, 0, me.Height - pic.Height		  end if		  		  g.ForeColor = &c000000		  g.DrawRect 0, 0, me.Width, me.Height		End Sub	#tag EndEvent	#tag Event		Sub Open()		  ActorSckt.Yield = true		End Sub	#tag EndEvent#tag EndEvents#tag Events ActorTimer	#tag Event		Sub Action()		  ActorSckt.Yield = true		  if lstActors.ListIndex > -1 then		    if lstActors.Cell( lstActors.ListIndex, 2 ).Left(4) = "http" then		      ActorIMG = Picture.FromData( ActorSckt.Get( lstActors.Cell( lstActors.ListIndex, 2 ), 30 ) )		      ActorSckt.close		    end if		    pgWheelActors.Visible = False		    cvsActorThumb.Refresh(false)		  end if		End Sub	#tag EndEvent#tag EndEvents#tag Events lstActors	#tag Event		Sub Open()		  me.ColumnType(0) = me.TypeEditable		  me.ColumnType(1) = me.TypeEditable		  me.ColumnType(2) = me.TypeEditable		End Sub	#tag EndEvent	#tag Event		Sub Change()		  		  pgWheelActors.Visible = False		  		  if me.ListIndex <> ActorLastSelection then		    ActorIMG = nil		    cvsActorThumb.Refresh(false)		    		    if me.Cell(me.ListIndex, 2) <> "" then		      pgWheelActors.Visible = true		      ActorTimer.Mode = 1		    end if		    		    ActorLastSelection = me.ListIndex		  end if		  		  if me.ListIndex <> -1 then		    AddRemoveActorButtons.RemoveEnabled = True		  else		    AddRemoveActorButtons.RemoveEnabled = False		  end if		  AddRemoveActorButtons.Refresh(false)		End Sub	#tag EndEvent#tag EndEvents#tag Events edtActorName	#tag Event		Function KeyDown(Key As String) As Boolean		  if Keyboard.AsyncKeyDown(&h4C) or Keyboard.AsyncKeyDown(&h24) then		    if me.Active and me.text <> "" then		      lstActors.AddRow edtActorName.Text, edtActorRole.Text, edtActorThumbURL.Text		      edtActor