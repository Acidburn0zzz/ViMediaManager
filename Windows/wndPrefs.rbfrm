e = false		    btnSave.Visible = false		    		    // Hide the pill button		    declare function ChangeWindowAttributes lib "Carbon" (w as WindowPtr, setAttrs as Integer, clearAttrs as Integer) as Integer		    dim err as Integer		    err = ChangeWindowAttributes(self,0,2^6)		    		    self.Height = sepSize(PrefsLastItem).Top		  #else		    self.Height = 51 + sepSize(PrefsLastItem).Top		  #endif		  		  'self.FullScreenAuxiliaryMBS = true		End Sub	#tag EndEvent	#tag Event		Sub Resized()		  #if TargetMacOS then		    ppPrefs.Height = sepSize(ppPrefs.Value).Top		  #else		    ppPrefs.Height = self.Height - 51		  #endif		End Sub	#tag EndEvent	#tag MenuHandler		Function FileCloseWindow() As Boolean Handles FileCloseWindow.Action			close			Return True					End Function	#tag EndMenuHandler	#tag MenuHandler		Function WindowMinimize() As Boolean Handles WindowMinimize.Action			self.Minimize			Return True					End Function	#tag EndMenuHandler	#tag Method, Flags = &h0		Sub Add2List(lstBox as listbox)		  Dim dlg as SelectFolderDialog		  Dim f as FolderItem		  Dim pic as Picture		  		  dlg = new SelectFolderDialog		  f = dlg.ShowModalWithin(self)		  		  if f <> nil then		    pic = f.IconImageMBS(16)		    pic.Mask = f.IconMaskMBS(16)		    		    lstBox.AddRow		    lstBox.Cell( lstBox.LastIndex, 0)  = f.Name		    lstBox.Cell( lstBox.LastIndex, 1 ) = f.AbsolutePath		    lstBox.RowPicture( lstBox.LastIndex ) = pic		    		    if lstBox = lstTVShowPaths then		      TVShowPaths.Append f.Name + "|" + f.AbsolutePath		    elseif lstBox = lstMoviePaths then		      MoviePaths.Append f.Name + "|" + f.AbsolutePath		    elseif lstBox = lstAnimePaths then		      AnimePaths.Append f.Name + "|" + f.AbsolutePath		    end if		    		  end if		  		Exception err as NilObjectException		  MsgBox "File not found."		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub RemoveFromList(lstBox as Listbox)		  dim i as integer		  		  if l