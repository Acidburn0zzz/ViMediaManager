#tag ClassProtected Class TbMainInherits Toolbar	#tag Event		Sub Action(item As ToolItem)		  		  Select Case item.Name		    		  Case "Movies"		    me.tbMovies.Pushed = True		    		  Case "TVShow"		    me.tbTVShow.Pushed = True		    		  Case "Anime"		    me.tbAnime.Pushed = True		    		    ' ----------------		  Case "ScrapeInfoAll"		    		    		  Case "ScrapeInfo"		    		    		  Case "ManualSearch"		    		    		  Case "ExtraArt"		    		    		    ' ----------------		  Case "EditMetadata"		    		    		  Case "RefreshList"		    		    		  Case "Clean"		    		    		    ' ----------------		  Case "Donate"		    ShowURL App.DonationURL		    		  End Select		End Sub	#tag EndEvent	#tag Event		Sub Open()		  SetToolItems		  RaiseEvent Open		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub SetToolItems()		  tbSeparator       = new ToolButton		  tbSeparator.Style = ToolButton.ToolStyleSeparator		  tbSeparator.Name  = "Separator"		  		  tbSpace       = new ToolButton		  tbSpace.Style = ToolButton.ToolStyleSpace		  tbSpace.Name  = "Space"		  		  tbDynamicSpace       = new ToolButton		  tbDynamicSpace.Style = ToolButton.ToolStyleFlexibleSpace		  tbDynamicSpace.Name  = "Dynamic Space"		  		  // Movies		  tbMovies         = new ToolButton		  tbMovies.Icon    = ToolMovies		  tbMovies.Caption = Localizable.Movies		  tbMovies.Name    = "Movies"		  tbMovies.HelpTag = Localizable.Movies		  tbMovies.Style   = ToolButton.ToolStyleToggleButton		  tbMovies.Pushed  = True		  		  // TV Shows		  tbTVShow          = new ToolButton		  tbTVShow.Icon     = ToolTVShows		  tbTVShow.Caption  = Localizable.TVShows		  tbTVShow.Name     = "TVShow"		  tbTVShow.HelpTag  = Localizable.TVShows		  tbTVShow.Style    = ToolButton.ToolStyleToggleButton		  tbTVShow.Pushed   = False		  		  // Anime		  tbAnime          = new ToolButton		  tbAnime.Icon     = ToolMonitor		  tbAnime.Caption  = Localizable.Anime		  tbAnime.Name     = "Anime"		  tbAnime.HelpTag  = Localizable.Anime		  tbAnime.Style    = ToolButton.ToolStyleToggleButton		  tbAnime.Pushed   = False		  		  // -------------		  		  // Fetch All Metadata		  tbFetchAllMetadata         = new ToolButton		  tbFetchAllMetadata.Icon    = ToolDownloadAll		  tbFetchAllMetadata.Caption = Localizable.FetchAllMetadata		  tbFetchAllMetadata.Name    = "DownloadAllMetadata"		  'tbFetchAllMetadata.HelpTag = "Download all metadata"		  tbFetchAllMetadata.Style   = ToolButton.ToolStylePushButton		  		  // Fetch Metadata		  tbFetchMetadata         = new ToolButton		  tbFetchMetadata.Icon    = ToolDownload		  tbFetchMetadata.Caption = Localizable.FetchMetadata		  tbFetchMetadata.Name    = "DownloadMetadata"		  'tbFetchMetadata.HelpTag = "Download metadata"		  tbFetchMetadata.Style   = ToolButton.ToolStylePushButton		  		  // Fetch Metadata		  tbManualSearch         = new ToolButton		  tbManualSearch.Icon    = ToolSearch		  tbManualSearch.Caption = Localizable.ManualSearch		  tbManualSearch.Name    = "ManualSearch"		  'tbManualSearch.HelpTag = "Manual Search"		  tbManualSearch.Style   = ToolButton.ToolStylePushButton		  		  // Extra Art		  tbExtraArt         = new ToolButton		  tbExtraArt.Icon    = ToolFanart		  tbExtraArt.Caption = Localizable.ExtraArt		  tbExtraArt.Name    = "ExtraArt"		  'tbExtraArt.HelpTag = "Download Extra Art"		  tbExtraArt.Style   = ToolButton.ToolStyleSeparateDropDown		  		  // -------------		  		  // Edit Metadata		  tbEditMetadata         = new ToolButton		  tbEditMetadata.Icon    = ToolInfo		  tbEditMetadata.Caption = Localizable.Metadata		  tbEditMetadata.Name    = "EditMetadata"		  'tbEditMetadata.HelpTag = "Edit Metadata"		  tbEditMetadata.Style   = ToolButton.ToolStylePushButton		  		  // Reload List		  tbReloadList         = new ToolButton		  tbReloadList.Icon    = ToolRefresh		  tbReloadList.Caption = Localizable.Refresh		  tbReloadList.Name    = "RefreshList"		  'tbReloadList.HelpTag = "Reload List"		  tbReloadList.Style   = ToolButton.ToolStylePushButton		  		  // Clean		  tbClean         = new ToolButton		  tbClean.Icon    = ToolClean		  tbClean.Caption = Localizable.Clean		  tbClean.Name    = "Clean"		  'tbClean.HelpTag = "Clean"		  tbClean.Style   = ToolButton.ToolStylePushButton		  		  // Donate		  tbDonate         = new ToolButton		  tbDonate.Icon    = ToolHeart		  tbDonate.Caption = Localizable.Donate		  tbDonate.Name    = "Donate"		  'tbDonate.HelpTag = "Donate"		  tbDonate.Style   = ToolButton.ToolStylePushButton		  		  // Append Toolbar items		  me.Append tbMovies		  me.Append tbTVShow		  me.Append tbAnime		  		  If IsLion then me.Append tbSpace else me.Append tbSeparator		  		  me.Append tbFetchAllMetadata		  me.Append tbFetchMetadata		  me.Append tbManualSearch		  me.Append tbExtraArt		  		  If IsLion then me.Append tbSpace else me.Append tbSeparator		  		  me.Append tbEditMetadata		  me.Append tbReloadList		  me.Append tbClean		  		  me.Append tbDynamicSpace		  		  me.Append tbDonate		End Sub	#tag EndMethod	#tag Hook, Flags = &h0		Event Open()	#tag EndHook	#tag Property, Flags = &h0		tbAnime As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbClean As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbDonate As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbDynamicSpace As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbEditMetadata As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbExtraArt As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbFetchAllMetadata As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbFetchMetadata As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbManualSearch As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbMovies As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbReloadList As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbSeparator As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbSpace As ToolButton	#tag EndProperty	#tag Property, Flags = &h0		tbTVShow As ToolButton	#tag EndProperty	#tag ViewBehavior		#tag ViewProperty			Name="Enabled"			Visible=true			Group="Appearance"			InitialValue="True"			Type="Boolean"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Height"			Visible=true			Group="Position"			InitialValue="32"			Type="Integer"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Index"			Visible=true			Group="ID"			Type="Integer"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="InitialParent"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Left"			Visible=true			Group="Position"			Type="Integer"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Name"			Visible=true			Group="ID"			Type="String"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Super"			Visible=true			Group="ID"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Top"			Visible=true			Group="Position"			Type="Integer"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Visible"			Visible=true			Group="Appearance"			InitialValue="True"			Type="Boolean"			InheritedFrom="Toolbar"		#tag EndViewProperty		#tag ViewProperty			Name="Width"			Visible=true			Group="Position"			InitialValue="100"			Type="Integer"			InheritedFrom="Toolbar"		#tag EndViewProperty	#tag EndViewBehaviorEnd Class#tag EndClass