nition  = \"Remove Folder"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Verwijder map"		#Tag Instance, Platform = Any, Language = sv, Definition  = \"Ta bort mapp"		#Tag Instance, Platform = Any, Language = de, Definition  = \"Ordner entfernen"	#tag EndConstant	#tag Constant, Name = msgIMDBApi, Type = String, Dynamic = True, Default = \"<AppName> uses an unofficial API for IMDb information. (http://imdbapi.com/)\r\rBecause of this\x2C IMDb information will only be available in english.", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"<AppName> uses an unofficial API for IMDb information. (http://imdbapi.com/)\r\rBecause of this\x2C IMDb information will only be available in english."		#Tag Instance, Platform = Any, Language = nl, Definition  = \"<AppName> gebruikt een onoffici\xC3\xABle API voor IMDb informatie. (http://imdbapi.com/)\r\rHierdoor is de IMDb informatie alleen beschikbaar in engels."		#Tag Instance, Platform = Any, Language = sv, Definition  = \"<AppName> anv\xC3\xA4nder en inofficiell API f\xC3\xB6r att h\xC3\xA4mta information fr\xC3\xA5n IMDb (http://imdbapi.com/).\r\rP\xC3\xA5 grund av det finns IMDb-data endast p\xC3\xA5 engelska."		#Tag Instance, Platform = Any, Language = de, Definition  = \"<AppName> benutzt ein unoffizielles API f\xC3\xBCr die Abfrage von IMDb Informationen (http://imdbapi.com). Daher liegen die IMDb Informationen nur in englischer Sprache vor."	#tag EndConstant#tag EndWindowCode#tag Events tbPrefs	#tag Event		Sub Action(item As ToolItem)		  Select case item.Name.ReplaceAll("tb", "")		  case "General"		    ppPrefs.Value = 0		  case "Movies"		    ppPrefs.Value = 1		  case "TVShows"		    ppPrefs.Value = 2		  case "Anime"		    ppPrefs.Value = 3		  case "Update"		    ppPrefs.Value = 4		  End Select		  		  ToolButton( me.Item( ppPrefs.Value + 1 ) ).Pushed = True		End Sub	#tag EndEvent#tag EndEvents#tag Events ppPrefs	#tag Event		Sub Change()		  Prefs.integerForKey("NSWindow Preferences Tab") = me.Value		  		  #If TargetCarbon Then SmoothResize( self, Self.Width, SepArray( ppPrefs.Value ).Top + 1, 4 )		  Self.Height = SepArray( ppPrefs.Value ).Top		End Sub	#tag EndEvent#tag EndEvents#tag Events lblArray	#tag Event		Sub Open(index as Integer)		  If DebugBuild Then Me.Visible = True		  Me.Text = Me.Text.ReplaceAll( "<AppName>", App.kAppName )		End Sub	#tag EndEvent#tag EndEvents#tag Events popDBLanguage	#tag Event		Sub Change()		  Prefs.setStringValue("DBLanguage"), FlagLanguage( me.Text )		End Sub	#tag EndEvent	#tag Event		Sub Open()		  		  For i as Integer = 0 to Me.ListCount - 1		    If FlagLanguage( Prefs.textStringForKey("DBLanguage") ) = Me.List( i ) Then Me.ListIndex = i		  Next		End Sub	#tag EndEvent#tag EndEvents#tag Events chkAutoDownExtras	#tag Event		Sub Open(index as Integer)		  chkAutoDownExtras(0).Value = Prefs.boolForKey("AutoDownloadPoster")		  chkAutoDownExtras(1).Value = Prefs.boolForKey("AutoDownloadFanart")		  chkAutoDownExtras(2).Value = Prefs.boolForKey("AutoDownloadBanner")		  chkAutoDownExtras(3).Value = Prefs.boolForKey("AutoDownloadExtraThumbs")		  		  chkAutoDownExtras(4).Value = Prefs.boolForKey("AutoDownloadClearArt")		  chkAutoDownExtras(5).Value = Prefs.boolForKey("AutoDownloadCharacterArt")		  chkAutoDownExtras(6).Value = Prefs.boolForKey("AutoDownloadLogo")		  chkAutoDownExtras(7).Value = Prefs.boolForKey("AutoDownloadThumb")		  chkAutoDownExtras(8).Value = Prefs.boolForKey("AutoDownloadDiscArt")		  chkAutoDownExtras(9).Value = Prefs.boolForKey("AutoDownloadTrailer")		  		  chkAutoDownExtras(10).Value = Prefs.boolForKey("AutoDownloadExtraFanart")		  		  If NOT DebugBuild Then chkAutoDownExtras(9).Visible = False Else chkAutoDownExtras(9).Enabled = True		End Sub	#tag EndEvent	#tag Event		Sub Action(index as Integer)		  Select Case Index		  case 0		    Prefs.boolForKey("AutoDownloadPoster")  = chkAutoDownExtras(0).Value		  case 1		    Prefs.boolForKey("AutoDownloadFanart")  = chkAutoDownExtras(1).Value		  case 2		    Prefs.boolForKey("AutoDownloadBanner")  = chkAutoDownExtras(2).Value		  case 3		    Prefs.boolForKey("AutoDownloadExtraThumbs") = chkAutoDownExtras(3).Value		    		  case 4		    Prefs.boolForKey("AutoDownloadClearArt")     = chkAutoDownExtras(4).Value		  case 5		    Prefs.boolForKey("AutoDownloadCharacterArt") = chkAutoDownExtras(5).Value		  case 6		    Prefs.boolForKey("AutoDownloadLogo")         = chkAutoDownExtras(6).Value		  case 7		    Prefs.boolForKey("AutoDownloadThumb")      = chkAutoDownExtras(7).Value		    		  case 8		    Prefs.boolForKey("AutoDownloadDiscArt")      = chkAutoDownExtras(8).Value		    		  Case 9		    Prefs.boolForKey("AutoDownloadTrailer") = chkAutoDownExtras(9).Value		    		    		  Case 10		    Prefs.boolForKey("AutoDownloadExtraFanart") = chkAutoDownExtras(10).Value		    		  End Select		End Sub	#tag EndEvent	#tag Event		Function MouseDown(index as Integer, X As Integer, Y As Integer) As Boolean		  If Keyboard.AsyncAltKey Then Return True		End Function	#tag EndEvent	#tag Event		Function MouseUp(index as Integer, X As Integer, Y As Integer) As Boolean		  Dim b as Boolean = NOT Me.Value		  For i as Integer = 0 to 10		    If i <> 9 Then chkAutoDownExtras(i).Value = b		  Next		End Function	#tag EndEvent#tag EndEvents#tag Events chkUpdate	#tag Event		Sub Open()		  me.Value = Prefs.boolForKey("SUCheckAtStartup")		End Sub	#tag EndEvent	#tag Event		Sub Action()		  Prefs.boolForKey("SUCheckAtStartup") = me.Value		End Sub	#tag EndEvent#tag EndEvents#tag Events btnCheckNow	#tag Event		Sub Action()		  Sparkle.CheckForUpdates True		End Sub	#tag EndEvent#tag EndEvents#tag Events lstMedia	#tag Event		Sub Open(index as Integer)		  Dim CurrentDict as Dictionary		  Dim f as FolderItem		  		  Select case index		  case 0		    CurrentDict = Prefs.dictionaryForKey("MoviePaths")		  case 1		    CurrentDict = Prefs.dictionaryForKey("TVShowPaths")		  case 2		    CurrentDict = Prefs.dictionaryForKey("AnimePaths")		  End Select		  		  If CurrentDict <> Nil then		    For Each Key as Variant in CurrentDict.Keys		      Dim Value as Variant = CurrentDict.Value( Key )		      		      lstMedia(index).AddRow Trim( Value ), Trim( Key )		      f = GetFolderItem( Trim( Key ) )		      If f <> Nil and f.Exists Then lstMedia(index).RowPicture( lstMedia(index).LastIndex ) = f.icon(16)		    Next		  End If		  		  		  'me.ColumnType(0) = me.TypeEditable		  'me.ColumnType(1) = me.TypeEditable		  me.AcceptFileDrop("special/folder")		End Sub	#tag EndEvent	#tag Event		Sub Change(index as Integer)		  arList(index).RemoveEnabled = lstMedia(index).ListIndex > -1		End Sub	#tag EndEvent	#tag Event		Sub CellTextChange(index as Integer, row as Integer, column as Integer)		  AddMediaToPrefs index		  RefreshList = True		End Sub	#tag EndEvent	#tag Event		Function KeyDown(index as Integer, Key As String) As Boolean		  If me.ListIndex > -1 And Keyboard.AsyncCommandKey And Keyboard.AsyncKeyDown(&h33) then		    lstMedia(index).RemoveRow lstMedia(index).ListIndex		    AddMediaToPrefs index		    RefreshList = True		  End if		End Function	#tag EndEvent	#tag Event		Function ConstructContextualMenu(index as Integer, base as MenuItem, x as Integer, y as Integer) As Boolean		  Dim miAdd as new MenuItem( MenuAddDir )		  Dim miRemove as new MenuItem ( MenuRemoveDir )		  base.Append miAdd		  If me.ListIndex > -1 then base.Append miRemove		  Return True		End Function	#tag EndEvent	#tag Event		Function ContextualMenuAction(index as Integer, hitItem as MenuItem) As Boolean		  If hitItem <> nil then		    Select case hitItem.Text		    Case MenuAddDir		      AddtoMediaList index		    case MenuRemoveDir		      RemoveFromMediaList index		    End Select		  End If		End Function	#tag EndEvent	#tag Event		Sub DropObject(index as Integer, obj As DragItem, a