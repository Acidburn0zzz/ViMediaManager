ownExtras(6).Value		  case 7		    Prefs.boolForKey("AutoDownloadTVThumb")      = chkAutoDownExtras(7).Value		  End Select		End Sub	#tag EndEvent#tag EndEvents#tag Events chkUpdate	#tag Event		Sub Open()		  me.Value = Prefs.boolForKey("SUCheckAtStartup")		End Sub	#tag EndEvent	#tag Event		Sub Action()		  Prefs.boolForKey("SUCheckAtStartup") = me.Value		End Sub	#tag EndEvent#tag EndEvents#tag Events btnCheckNow	#tag Event		Sub Action()		  Sparkle.CheckForUpdates True		End Sub	#tag EndEvent#tag EndEvents#tag Events lstMedia	#tag Event		Sub Open(index as Integer)		  Dim CurrentDict as Dictionary		  Dim f as FolderItem		  		  Select case index		  case 0		    CurrentDict = Prefs.dictionaryForKey("MoviePaths")		  case 1		    CurrentDict = Prefs.dictionaryForKey("TVShowPaths")		  case 2		    CurrentDict = Prefs.dictionaryForKey("AnimePaths")		  End Select		  		  If CurrentDict <> Nil then		    For Each Key as Variant in CurrentDict.Keys		      Dim Value as Variant = CurrentDict.Value( Key )		      		      lstMedia(index).AddRow Trim( Key ), Trim( Value )		      f = GetFolderItem( Trim( Value ) )		      lstMedia(index).RowPicture( lstMedia(index).LastIndex ) = f.icon(16)		    Next		  End If		  		  		  'me.ColumnType(0) = me.TypeEditable		  'me.ColumnType(1) = me.TypeEditable		  me.AcceptFileDrop("special/folder")		End Sub	#tag EndEvent	#tag Event		Sub Change(index as Integer)		  arList(index).RemoveEnabled = lstMedia(index).ListIndex > -1		End Sub	#tag EndEvent	#tag Event		Sub CellTextChange(index as Integer, row as Integer, column as Integer)		  AddMediaToPrefs index		  RefreshList = True		End Sub	#tag EndEvent	#tag Event		Function KeyDown(index as Integer, Key As String) As Boolean		  If me.ListIndex > -1 And Keyboard.AsyncCommandKey And Keyboard.AsyncKeyDown(&h33) then		    lstMedia(index).RemoveRow lstMedia(index).ListIndex		    AddMediaToPrefs index		    RefreshList = True		  End if		End Function	#tag EndEvent	#tag Event		Function ConstructContextualMenu(index as Integer, base as MenuItem, x as Integer, y as Integer) As Boolean		  Dim miAdd as new MenuItem( MenuAddDir )		  Dim miRemove as new MenuItem ( MenuRemoveDir )		  base.Append miAdd		  If me.ListIndex > -1 then base.Append miRemove		  Return True		End Function	#tag EndEvent	#tag Event		Function ContextualMenuAction(index as Integer, hitItem as MenuItem) As Boolean		  If hitItem <> nil then		    Select case hitItem.Text		    Case MenuAddDir		      AddtoMediaList index		    case MenuRemoveDir		      RemoveFromMediaList index		    End Select		  End If		End Function	#tag EndEvent	#tag Event		Sub DropObject(index as Integer, obj As DragItem, action As Integer)		  Dim f as FolderItem		  While obj.FolderItemAvailable		    f = obj.FolderItem		    lstMedia(index).AddRow f.Name, f.AbsolutePath		    lstMedia(index).RowPicture( lstMedia(index).LastIndex ) = f.Icon(16)		    If Not obj.NextItem then Exit		  Wend		  		  AddMediaToPrefs index		  RefreshList = True		End Sub	#tag EndEvent	#tag Event		Sub DoubleClick(index as Integer)		  Dim row, column as Integer		  row =    Me.RowFromXY(    System.MouseX - Me.Left - Self.Left, System.MouseY - Me.Top - Self.Top )		  column = Me.ColumnFromXY( System.MouseX - Me.Left - Self.Left, System.MouseY - Me.Top - Self.Top )		  Me.EditCell( row, column )		End Sub	#tag EndEvent#tag EndEvents#tag Events arList	#tag Event		Sub ActionAdd(index as Integer)		  AddToMediaList index		End Sub	#tag EndEvent	#tag Event		Sub ActionRemove(index as Integer)		  RemoveFromMediaList index		End Sub	#tag EndEvent#tag EndEvents#tag Events txtRenamePattern	#tag Event		Sub Open(index as Integer)		  Select case index		  case 0		    txtRenamePattern(index).Text = Prefs.textStringForKey("RenameStringMovies")		  case 1		    txtRenamePattern(index).Text = Prefs.textStringForKey("RenameStringTVShows")		  case 2		    txtRenamePattern(index).Text = Prefs.textStringForKey("RenameStringAnime")		  End Select		End Sub	#tag EndEvent#tag EndEvents#tag Events btnRenameHelp	#tag Event		Sub Action(index as Integer)		  If index > 0 then		    MsgBox msgRenamePatternSeries		  Else		    MsgBox msgRenamePatternMovies		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events popMovieNFOName	#tag Event		Sub Change()		  Prefs.setStringValue "FileNameNFO", me.Text		  FileNameNFO = me.Text		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Select Case Prefs.textStringForKey("FileNameNFO")		  Case "<movie>.nfo"		    me.ListIndex = 0		  Case "movie.nfo"		    me.ListIndex = 1		  End Select		End Sub	#tag EndEvent#tag EndEvents#tag Events popMoviePosterName	#tag Event		Sub Change()		  Prefs.setStringValue "FileNamePoster", me.Text		  FileNamePoster = me.Text		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Select Case Prefs.textStringForKey("FileNamePoster")		  Case "poster.jpg"		    me.ListIndex = 0		  Case "poster.tbn"		    me.ListIndex = 1		  Case "<movie>.jpg"		    me.ListIndex = 2		  Case "<movie>.tbn"		    me.ListIndex = 3		  Case "movie.jpg"		    me.ListIndex = 4		  Case "movie.tbn"		    me.ListIndex = 5		  Case "folder.jpg"		    me.ListIndex = 6		  End Select		End Sub	#tag EndEvent#tag EndEvents#tag Events popMovieFanartName	#tag Event		Sub Change()		  Prefs.setStringValue "FileNameFanart", me.Text		  FileNameFanart = me.Text		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Select Case Prefs.textStringForKey("FileNameFanart")		  Case "fanart.jpg"		    me.ListIndex = 0		  Case "<movie>-fanart.jpg"		    me.ListIndex = 1		  Case "<movie>.fanart.jpg"		    me.ListIndex = 2		  End Select		End Sub	#tag EndEvent#tag EndEvents#tag Events popMovieTrailerName	#tag Event		Sub Change()		  Prefs.setStringValue "FileNameTrailer", me.Text		  FileNameTrailer = me.Text		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Select Case Prefs.textStringForKe