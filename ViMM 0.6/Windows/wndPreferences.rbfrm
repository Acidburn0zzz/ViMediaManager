efs.boolForKey( "ShowMiniIcon-Thumb" )          = chkMiniConVisible(9).Value		  Case 10		    Prefs.boolForKey( "ShowMiniIcon-DiscArt" )        = chkMiniConVisible(10).Value		    		  Case 11		    Prefs.boolForKey( "ShowMiniIcon-Subtitles" )      = chkMiniConVisible(11).Value		  Case 12		    Prefs.boolForKey( "ShowMiniIcon-Resolution" )     = chkMiniConVisible(12).Value		    		  End Select		End Sub	#tag EndEvent	#tag Event		Sub Open(index as Integer)		  chkMiniConVisible(0).Value  = Prefs.boolForKey( "ShowMiniIcon-Poster" )		  chkMiniConVisible(1).Value  = Prefs.boolForKey( "ShowMiniIcon-Backdrop" )		  chkMiniConVisible(2).Value  = Prefs.boolForKey( "ShowMiniIcon-Banner" )		  chkMiniConVisible(3).Value  = Prefs.boolForKey( "ShowMiniIcon-ExtraThumbs" )		  chkMiniConVisible(4).Value  = Prefs.boolForKey( "ShowMiniIcon-ExtraBackdrops" )		  chkMiniConVisible(5).Value  = Prefs.boolForKey( "ShowMiniIcon-Trailer" )		  chkMiniConVisible(6).Value  = Prefs.boolForKey( "ShowMiniIcon-ClearArt" )		  chkMiniConVisible(7).Value  = Prefs.boolForKey( "ShowMiniIcon-CharacterArt" )		  chkMiniConVisible(8).Value  = Prefs.boolForKey( "ShowMiniIcon-Logo" )		  chkMiniConVisible(9).Value  = Prefs.boolForKey( "ShowMiniIcon-Thumb" )		  chkMiniConVisible(10).Value = Prefs.boolForKey( "ShowMiniIcon-DiscArt" )		  chkMiniConVisible(11).Value = Prefs.boolForKey( "ShowMiniIcon-Subtitles" )		  chkMiniConVisible(12).Value = Prefs.boolForKey( "ShowMiniIcon-Resolution" )		End Sub	#tag EndEvent	#tag Event		Function MouseUp(index as Integer, X As Integer, Y As Integer) As Boolean		  Dim b as Boolean = NOT Me.Value		  For i as Integer = 0 to 12		    chkMiniConVisible(i).Value = b		  Next		End Function	#tag EndEvent	#tag Event		Function MouseDown(index as Integer, X As Integer, Y As Integer) As Boolean		  If Keyboard.AsyncAltKey Then Return True		End Function	#tag EndEvent#tag EndEvents