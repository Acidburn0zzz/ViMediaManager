tr.Rating )		  edtPremiere.Text      = MovieAttr.DatePremiered		  edtYear.Text          = str( MovieAttr.DateYear )		  		  edtDirector.Text      = MovieAttr.CreditDirector		  edtWriter.Text        = MovieAttr.CreditWriter		  		  // Studios		  lstStudios.DeleteAllRows		  For i = 0 to MovieAttr.Studios.Ubound		    lstStudios.AddRow MovieAttr.Studios(i)		  Next		  		  // Genres		  Dim AllGenres as String		  For i = 0 to MovieAttr.Genres.Ubound		    If i <> MovieAttr.Genres.Ubound then		      AllGenres = AllGenres + MovieAttr.Genres(i) + ", "		    Else		      AllGenres = AllGenres + MovieAttr.Genres(i)		    End If		  Next		  edtGenres.Text = AllGenres		  		  // Actors		  lstActors.DeleteAllRows		  For i = 0 to MovieAttr.ActorName.Ubound		    lstActors.AddRow MovieAttr.ActorName(i), MovieAttr.ActorRole(i), MovieAttr.ActorThumbURL(i)		  Next		  		  // Path & Size		  Dim MovieFile as FolderItem = FindMovieItem( MovieParent )		  If MovieFile <> Nil and MovieFile.Exists then		    lblLocation.Text = MovieFile.AbsolutePath.ReplaceAll( ":", "/" )		    		    If MovieFile.Length > 0 then		      Dim GB, MB as Double		      MB = MovieFile.Length / 1000 / 1000		      GB = MB / 1000		      		      MB = Floor( MB * 100 ) / 100		      GB = Floor( GB * 100 ) / 100		      		      If MB > 1000 then lblSize.Text = str(GB) + " GB" else lblSize.Text = str(MB) + " MB"		    Else		      lblSize.text = ""		    End If		  End If		  		  // Width x Height		  If MovieAttr.InfoVideoWidth > -1 and MovieAttr.InfoVideoHeight > -1 then		    lblDimensions.Text = str( MovieAttr.InfoVideoWidth ) + " x " + str( MovieAttr.InfoVideoHeight )		  Else		    lblDimensions.Text = ""		  End If		  		  // Ratio		  If MovieAttr.InfoVideoAspect > -1 then lblRatio.Text = str( MovieAttr.InfoVideoAspect ) + ":1" else lblRatio.Text = ""		  		  // Runtime		  If MovieAttr.InfoVideoRuntime <> "" then lblRuntime.Text = Val( MovieAttr.InfoVideoRuntime ).Minutes2Hours else lblRuntime.Text = ""		  		  // IMDB		  If MovieAttr.ID_IMDB <> "" then		    lnkIMDB.Text = MovieAttr.ID_IMDB		    lnkIMDB.URL  = "http://www.imdb.com/title/" + MovieAttr.ID_IMDB		  Else		    lnkIMDB.Text = ""		    lnkIMDB.URL  = ""		  End If		  lnkIMDB.Invalidate( False )		  		  // TMDB		  If MovieAttr.ID_TMDB <> "" then		    lnkTMDB.Text = MovieAttr.ID_TMDB		    lnkTMDB.URL  = "http://www.themoviedb.org/movie/" + MovieAttr.ID_TMDB		  Else		    lnkTMDB.Text = ""		    lnkTMDB.URL  = ""		  End If		  lnkTMDB.Invalidate( False )		  		  // Poster		  Dim PosterFile as FolderItem = FindImagePoster( MovieParent )		  If PosterFile <> Nil and PosterFile.Exists then		    cvsPoster.PreviewArt = Picture.Open( PosterFile )		  Else		    cvsPoster.PreviewArt = Nil		    btnDeletePoster.Enabled = False		  End If		  cvsPoster.Invalidate( False )		  		  // Fanart		  Dim FanartFile as FolderItem = FindImagePoster( MovieParent )		  If FanartFile <> Nil and FanartFile.Exists then		    cvsFanart.PreviewArt = Picture.Open( FanartFile )		  Else		    cvsFanart.PreviewArt = Nil		    btnDeleteFanart.Enabled = False		  End If		  cvsFanart.Invalidate( False )		  		  // Trailer		  scktTrailer.Close		  		  btnDownloadTrailer.Enabled = True		  edtTrailerURL.Enabled = True		  edtTrailerURL.Text = ""		  		  btnDeleteTrailer.Enabled = FindTrailerItem( MovieParent ) <> Nil		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub GetYouTubeURL(XMLContent as String)		  Dim Xml as new XmlDocument		  Dim xroot, xnode as XmlNode		  Dim ID as String		  		  Xml.PreserveWhitespace = False		  Xml.LoadXml( XmlContent )		  		  xroot = XML.Child(0).Child(2).FirstChild		  		  If wndMain.ppMain.Value = 0 then		    ID = MovieAttr.ID_TMDB		  ElseIf wndMain.ppMain.Value = 2 then		    // ID = AnimeAttr.ID_TMDB		  End If		  		  For i as Integer = 0 to xroot.ChildCount - 1		    xnode = xroot.Child(i)		    		    If xnode.Name = "Trailer" and xnode.FirstChild = Nil Then		      MsgBox msgNoTrailer		      pgTrailer.Visible = False		      btnDeleteTrailer.Enabled = FindTrailerItem( MovieParent ) <> Nil		      		    ElseIf xnode.Name = "Trailer" and xnode.FirstChild <> Nil Then		      		      If xnode.FirstChild.Value <> "" then		        scktTrailer.Get( xnode.FirstChild.Value + "&hd=1" )		      ElseIf xnode.FirstChild.Value = "" or xnode.FirstChild = Nil Then		        MsgBox msgNoTrailer		        pgTrailer.Visible = False		        btnDeleteTrailer.Enabled = FindTrailerItem( MovieParent ) <> Nil		      End If		      		    End If		    		  Next		  		  		End Sub	#tag EndMethod	#tag Method, Flags = &h21		Private Sub SetNFO()		  		End Sub	#tag EndMethod	#tag Property, Flags = &h1		Protected ActorIMG As Picture	#tag EndProperty	#tag Property, Flags = &h1		Protected ActorLastSelection As Integer = -1	#tag EndProperty	#tag Property, Flags = &h0		MovieParent As FolderItem	#tag EndProperty	#tag Constant, Name = kCommaSeparated, Type = String, Dynamic = True, Default = \"comma separated", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"comma separated"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Scheiding met komma\'s"	#tag EndConstant	#tag Constant, Name = kDownloadComplete, Type = String, Dynamic = True, Default = \"Download Complete", Scope = Private		#Tag Instance, Platform = Any, Language = en, Definition  = \"Download completed"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Download compleet"	#tag EndConstant	#tag Constant, Name = kNoPosterAvailable, Type = String, Dynamic = True, Default = \"No Poster\rAvailable", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"No Poster\rAvailable"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Geen Poster\rBeschikbaar"	#tag EndConstant	#tag Constant, Name = lblTrailerMessage, Type = String, Dynamic = True, Default = \"If left empty\x2C ViMediaManager will try to find a trailer on it\'s own.", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"If left empty\x2C ViMediaManager will try to find a trailer on it\'s own."		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Als dit veld leeg gelaten wordt\x2C dan zal ViMediaManager zelf proberen een trailer te vinden."	#tag EndConstant	#tag Constant, Name = msgDeletePoster, Type = String, Dynamic = True, Default = \"Are you sure you want to delete the current poster\?", Scope = Protected		#Tag Instance, Platform = Any, Language = Default, Definition  = \"Are you sure you want to delete the current image\?"		#Tag Instance, Platform = Any, Language = Default, Definition  = \"Weet je zeker dat je het huidige plaatje wilt wissen\?"	#tag EndConstant	#tag Constant, Name = msgDeleteTrailer, Type = String, Dynamic = True, Default = \"Are you sure you want to delete the current trailer\?", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"Are you sure you want to delete the current trailer\?"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Weet je zeker dat je de huidige trailer wilt wissen\?"	#tag EndConstant	#tag Constant, Name = msgNothingToDelete, Type = String, Dynamic = True, Default = \"No image to delete", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"No image to delete"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Geen plaatje om te wissen"	#tag EndConstant	#tag Constant, Name = msgNoTrailer, Type = String, Dynamic = True, Default = \"No trailer found.", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"No trailer found."		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Geen trailer gevonden."	#tag EndConstant	#tag Constant, Name = msgTrailerExists, Type = String, Dynamic = True, Default = \"A trailer already exists\x2C do you want to replace it\?", Scope = Protected		#Tag Instance, Platform = Any, Language = en, Definition  = \"A trailer already exists\x2C do you want to replace it\?"		#Tag Instance, Platform = Any, Language = nl, Definition  = \"Er is al een trailer gevonden\x2C wil je deze vervangen\?"	#tag EndConstant#tag EndWindowCode#tag Events CancelSave	#tag Event		Sub ActionCancel()		  Close		End Sub	#tag EndEvent	#tag Event		Sub ActionOK()		  Close		End Sub	#tag EndEvent#tag EndEvents#tag Events btnPrev	#tag Event		Sub Action()		  If wndMain.lstMovies.ListIndex > 0 then		    SetNFO		    wndMain.lstMovies.UpdateRow		    		    wndMain.lstMovies.ListIndex = wndMain.lstMovies.ListIndex - 1		    GetNFO		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events btnNext	#tag Event		Sub Action()		  If wndMain.lstMovies.ListIndex < wndMain.lstMovies.ListCount - 1 then		    SetNFO		    wndMain.lstMovies.UpdateRow		    		    wndMain.lstMovies.ListIndex = wndMain.lstMovies.ListIndex + 1		    GetNFO		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events tpMetadata	#tag Event		Sub Open()		  Me.Value = Prefs.integerForKey("NSWindow MetadataMovie Tab")		End Sub	#tag EndEvent	#tag Event		Sub Change()		  Prefs.integerForKey("NSWindow MetadataMovie Tab") = Me.Value		  		  #If TargetMacOS then SmoothResize( self, self.Width, SeparatorArray( me.Value ).Top + 54, 4 )		  Height = SeparatorArray( me.Value ).Top + 55		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsSummaryPoster	#tag Event		Sub Paint(g As Graphics)		  If MovieAttr.ART_Poster <> Nil then		    Dim Pic as Picture = ScaleImage( MovieAttr.ART_Poster, Me.Width, Me.Height )		    g.DrawPicture Pic, 0, 0		    		    g.ForeColor = &c000000		    g.DrawRect 0, 0, Pic.Width, Pic.Height		  Else		    g.ForeColor = &cFFFFFF		    g.FillRect 0, 0, me.Width, me.Height		    		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Poster" + chr(13) + "Available", ( Me.Width / 2 ) - ( g.StringWidth( "No Poster" + chr(13) + "Available" ) / 4), ( Me.Height / 2 ) - ( g.TextSize * 2 ), Me.Width, False )		    		    g.ForeColor = &c000000		    g.DrawRect 0, 0, Me.Width, Me.Height		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events SeparatorArray	#tag Event		Sub Open(index as Integer)		  'Me.Visible = DebugBuild		  Me.Visible = False		End Sub	#tag EndEvent#tag EndEvents#tag Events udRating	#tag Event		Sub Down()		  If val( edtRating.Text ) > 0 then edtRating.Text = str( val( edtRating.Text ) - 0.1 )		End Sub	#tag EndEvent	#tag Event		Sub Up()		  If val( edtRating.Text ) < 10 then edtRating.Text = str( val( edtRating.Text ) + 0.1 )		End Sub	#tag EndEvent#tag EndEvents#tag Events lstStudios	#tag Event		Sub Change()		  AddRemoveStudio.RemoveEnabled = Me.ListIndex > -1		  'AddRemoveStudio.Invalidate		End Sub	#tag EndEvent#tag EndEvents#tag Events AddRemoveStudio	#tag Event		Sub ActionAdd()		  If edtStudios.Text <> "" then lstStudios.AddRow edtStudios.Text		  edtStudios.Text = ""		End Sub	#tag EndEvent	#tag Event		Sub ActionRemove()		  If lstStudios.ListIndex > -1 then lstStudios.RemoveRow lstStudios.ListIndex		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsActor	#tag Event		Sub Paint(g As Graphics)		  If ActorIMG <> Nil then		    Dim Pic as Picture  = ScaleImage( ActorIMG, Me.Width, Me.Height )		    g.DrawPicture Pic, 0, 0		    		    g.ForeColor = &c000000		    g.DrawRect 0, 0, Pic.Width, Pic.Height		  Else		    g.ForeColor = &cFFFFFF		    g.FillRect 0, 0, me.Width, me.Height		    		    g.ForeColor = &cAAAAAA		    g.DrawString( "No Image" + chr(13) + "Available", ( Me.Width / 2 ) - ( g.StringWidth( "No Image" + chr(13) + "Available" ) / 4), ( Me.Height / 2 ) - ( g.TextSize * 2 ), Me.Width, False )		    		    g.ForeColor = &c000000		    g.DrawRect 0, 0, Me.Width, Me.Height		  End If		End Sub	#tag EndEvent	#tag Event		Sub Open()		  ActorSckt.Yield = True		End Sub	#tag EndEvent#tag EndEvents#tag Events ActorSckt	#tag Event		Sub Error(code as integer)		  SocketError( code, Me )		  		Exception err as NilObjectException		  Me.Close		End Sub	#tag EndEvent#tag EndEvents#tag Events ActorTimer	#tag Event		Sub Action()		  If lstActors.ListIndex > -1 and lstActors.Cell( lstActors.ListIndex, 2 ).Left(4) = "http" then		    ActorIMG = Picture.FromData( ActorSckt.Get( lstActors.Cell( lstActors.ListIndex, 2 ), Prefs.integerForKey( "TimeOut" ) ) )		    ActorSckt.Close		  End If		  		  pgActor.Visible  =   False		  cvsActor.Invalidate( False )		End Sub	#tag EndEvent#tag EndEvents#tag Events AddRemoveActor	#tag Event		Sub ActionAdd()		  If edtActorName.Text <> "" then		    lstActors.AddRow edtActorName.Text, edtActorRole.Text, edtActorThumbURL.Text		    edtActorName.Text = ""		    edtActorRole.Text = ""		    edtActorThumbURL.Text = ""		  End If		End Sub	#tag EndEvent	#tag Event		Sub ActionRemove()		  If lstActors.ListIndex > -1 then lstActors.RemoveRow lstActors.ListIndex		End Sub	#tag EndEvent#tag EndEvents#tag Events lstActors	#tag Event		Sub Change()		  AddRemoveActor.RemoveEnabled = Me.ListIndex > -1		  pgActor.Visible = False		  		  If Me.ListIndex <> ActorLastSelection then		    ActorIMG = Nil		    cvsActor.Invalidate( False )		    		    If me.Cell( me.ListIndex, 2 ) <> "" then		      pgActor.Visible = True		      ActorTimer.Mode = 1		    End If		    		    ActorLastSelection = Me.ListIndex		  End If		End Sub	#tag EndEvent	#tag Event		Sub DoubleClick()		  Dim row, column as Integer		  row =    Me.RowFromXY(    System.MouseX - Me.Left - Self.Left, System.MouseY - Me.Top - Self.Top )		  column = Me.ColumnFromXY( System.MouseX - Me.Left - Self.Left, System.MouseY - Me.Top - Self.Top )		  Me.EditCell( row, column )		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsPoster	#tag Event		Sub Open()		  me.Mode = "Poster"		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsFanart	#tag Event		Sub Open()		  me.Mode = "Fanart"		End Sub	#tag EndEvent#tag EndEvents#tag Events btnOpenPoster	#tag Event		Sub Action()		  Dim TheFile as FolderItem = GetOpenFolderItem( "image/any" )		  If TheFile <> Nil and TheFile.Exists then cvsPoster.AddImage( TheFile )		End Sub	#tag EndEvent#tag EndEvents#tag Events btnDeletePoster	#tag Event		Sub Action()		  Dim TheFile as FolderItem = FindImagePoster( MovieParent )		  If TheFile <> Nil and TheFile.Exists then		    If dlgWindow( msgDeletePoster, Localizable.kCantBeUndone ) = 1 then		      TheFile.Delete		      MovieAttr.ART_Poster = Nil		      cvsPoster.Reload Nil		    End If		  Else		    MsgBox msgNothingToDelete		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events btnDeleteFanart	#tag Event		Sub Action()		  Dim TheFile as FolderItem = FindImageFanart( MovieParent )		  If TheFile <> Nil and TheFile.Exists then		    If dlgWindow( msgDeletePoster, Localizable.kCantBeUndone ) = 1 then		      TheFile.Delete		      MovieAttr.ART_Fanart = Nil		      cvsPoster.Reload Nil		    End If		  Else		    MsgBox msgNothingToDelete		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events btnOpenFanart	#tag Event		Sub Action()		  Dim TheFile as FolderItem = GetOpenFolderItem( "image/any" )		  If TheFile <> Nil and TheFile.Exists then cvsFanart.AddImage TheFile		End Sub	#tag EndEvent#tag EndEvents#tag Events btnDownloadTrailer	#tag Event		Sub Action()		  If edtTrailerURL.Text <> "" then		    scktTrailer.Get( edtTrailerURL.Text )		  Else		    pgTrailer.Visible = True		    		    If MovieAttr.ID_TMDB <> "" then		      scktTrailer.Get( "http://api.themoviedb.org/2.1/Movie.getInfo/" + Prefs.textStringForKey("DBLanguage") + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.ID_TMDB )		    ElseIf MovieAttr.ID_IMDB <> "" then		      scktTrailer.Get( "http://api.themoviedb.org/2.1/Movie.imdbLookup/" + Prefs.textStringForKey("DBLanguage") + "/xml/" + TMDB.ApiKey + "/" + MovieAttr.ID_IMDB )		    End If		    		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events btnDeleteTrailer	#tag Event		Sub Action()		  Dim TheFile as FolderItem = FindTrailerItem( MovieParent )		  If TheFile <> Nil and TheFile.Exists Then		    If dlgWindow( msgDeleteTrailer, Localizable.kCantBeUndone ) = 1 then TheFile.Delete		    Me.Enabled = TheFile.Exists		  Else		    MsgBox kNothingToDelete		  End If		End Sub	#tag EndEvent#tag EndEvents#tag Events scktTrailer	#tag Event		Sub Error(code as integer)		  SocketError( code, Me )		  		Exception err as NilObjectException		  Me.Close		End Sub	#tag EndEvent	#tag Event		Sub ReceiveProgress(bytesReceived as integer, totalBytes as integer, newData as string)		  pgTrailer.Maximum = totalBytes		  pgTrailer.Value = bytesReceived		  		  lblDownloadProgress.Text = str( floor( ( bytesReceived / 1024 / 1024 ) * 100 ) / 100 ) + " MB / " + str( floor( ( totalBytes / 1024 / 1024 ) * 100 ) / 100 ) + " MB"		End Sub	#tag EndEvent	#tag Event		Sub PageReceived(url as string, httpStatus as integer, headers as internetHeaders, content as string)		  Dim tmpContent as String = Content		  		  If url.InStr( 0, "themoviedb" ) > 0 and tmpContent.Left(5) = "<?xml" then		    GetYouTubeURL( tmpContent )		    		  ElseIf url.InStr( 0, "youtube" ) > 0 then		    		    Dim idx1, idx2 as Integer		    idx1 = tmpContent.InStr( "img.src = """ ) + 11  //extract the 'generate' url		    idx2 = tmpContent.InStr( idx1, ";" )		    Dim s as String = tmpContent.Mid( idx1, idx2 - idx1 - 1 )		    		    s = s.ReplaceAll("\/", "/")         //unencode these characters		    s = s.ReplaceAll("\u0026", "&")		    		    s = s.Replace("generate_204", "videoplayback")   //switch generate to video		    s = s.Replace("pmbypass&", "")                   //remove these bypass things		    s = s.Replace("pmbypass=yes&", "")		    		    If s <> "" then		      pgTrailer.Visible = True		      edtTrailerURL.Enabled = False		      btnDownloadTrailer.Enabled = False		      lblDownloadProgress.Visible = True		      		      Me.Get s, SpecialFolder.Temporary.Child( "trailer.downloading" )		    Else		      MsgBox msgNoTrailer		      pgTrailer.Visible = False		    End If		    		  Else		    MsgBox msgNoTrailer		    // Me.Get s, SpecialFolder.Temporary.Child( "trailer.downloading" )		  End If		End Sub	#tag EndEvent	#tag Event		Sub DownloadComplete(url as string, httpStatus as integer, headers as internetHeaders, file as folderItem)		  Dim MovieFile, TrailerFile, Destination as FolderItem		  MovieFile = FindMovieItem( MovieParent )		  TrailerFile = FindTrailerItem( MovieParent )		  me.Close		  		  // If file seems trailer like.		  If File <> Nil and File.Exists and File.Length > 1024 then		    		    // Trailer Destination		    If MovieFile <> Nil and MovieFile.Exists then		      Destination = MovieParent.Child( Prefs.textStringForKey("FileNameTrailer").ReplaceAll( "<movie>", MovieFile.NameWithoutExtension ) + file.VideoFileExtension )		    Else		      Destination = MovieParent.Child( "trailer" + file.VideoFileExtension )		    End If		    		    If Destination <> Nil then		      If TrailerFile <> Nil and TrailerFile.Exists then		        If dlgWindow( msgDeleteTrailer, kCantBeUndone ) = 1 then File.MoveFileTo Destination		      Else		        File.MoveFileTo Destination		      End If		    End If		    		    MsgBox kDownloadComplete		  Else		    MsgBox Localizable.kError + ": " + str( Me.LastErrorCode ) + chr(13) + chr(13) + Localizable.kDownloadFailed		  End If		  		  pgTrailer.Visible = False		  lblDownloadProgress.Visible = False		  		  btnDownloadTrailer.Enabled = True		  edtTrailerURL.Enabled = True		  edtTrailerURL.Text = ""		End Sub	#tag EndEvent#tag EndEvents