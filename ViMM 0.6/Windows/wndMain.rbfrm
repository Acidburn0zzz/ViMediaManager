		    		  ElseIf Index = 1 then		    		  ElseIf Index = 2 then		    		  End If		End Sub	#tag EndMethod#tag EndWindowCode#tag Events lstMovies	#tag Event		Function ConstructContextualMenu(base as MenuItem, x as Integer, y as Integer) As Boolean		  MenuConstructor base, Me		End Function	#tag EndEvent#tag EndEvents#tag Events ActBar	#tag Event		Sub Open(index as Integer)		  me.ClearAttachments		  		  Select case index		  case 0		    me.AttachLeft lstMovies		    me.AttachRight cvsMovies		    		  case 1		    		  case 2		    		  End Select		End Sub	#tag EndEvent	#tag Event		Sub Moved(index as Integer, Distance as Integer)		  Select case index		  case 0		    Prefs.integerForKey("NSMoviesSidebarWidth")  = me.Width		  case 1		    Prefs.integerForKey("NSTVShowsSidebarWidth") = me.Width		  case 2		    Prefs.integerForKey("NSAnimeSidebarWidth")   = me.Width		  End Select		End Sub	#tag EndEvent#tag EndEvents#tag Events cvsMovies	#tag Event		Sub Open()		  Me.Height = Height		End Sub	#tag EndEvent#tag EndEvents#tag Events btnTest	#tag Event		Sub Action()		  wndMetadataMovie.Show		End Sub	#tag EndEvent#tag EndEvents#tag Events MainTB	#tag Event		Sub Action(item As ToolItem)		  		  Select Case item.Name		    		  Case "Movies"		    SwitchPanels 0		    		  Case "TVShow"		    SwitchPanels 1		    		  Case "Anime"		    SwitchPanels 2		    		    ' ----------------		  Case "DownloadAllMetadata"		    		    		  Case "DownloadMetadata"		    		    		  Case "ManualSearch"		    		    		  Case "ExtraArt"		    		    		    ' ----------------		  Case "EditMetadata"		    		    		  Case "RefreshList"		    lstMovies.CreateList		    		  Case "Clean"		    		    		    ' ----------------		  Case "Donate"		    ShowURL App.DonationURL		    		  End Select		End Sub	#tag EndEvent#tag EndEvents