alog \xC3\xA4ndern."	#tag EndConstant#tag EndWindowCode#tag Events btnNext	#tag Event		Sub Action()		  ppSplash.Value = ppSplash.Value + 1		End Sub	#tag EndEvent#tag EndEvents#tag Events btnSkip	#tag Event		Sub Action()		  Close		End Sub	#tag EndEvent#tag EndEvents#tag Events btnFinish	#tag Event		Sub Action()		  Dim Movies as new Dictionary		  Dim TVShows as new Dictionary		  Dim Anime as new Dictionary		  Dim i, index as Integer		  Dim key, value as String		  		  For index = 0 to 2		    For i = 0 to lstArray( index ).ListCount - 1		      		      key   = Trim( lstArray( index ).Cell( i, 1 ) )		      value = Trim( lstArray( index ).Cell( i, 0 ) )		      		      Select case index		      case 0		        Movies.Value( key ) = value		      case 1		        TVShows.Value( key ) = value		      case 2		        Anime.Value( key ) = value		      End Select		      		    Next		    If index = 0 then Prefs.setDictionaryValue "MoviePaths",  Movies		    If index = 1 then Prefs.setDictionaryValue "TVShowPaths", TVShows		    If index = 2 then Prefs.setDictionaryValue "AnimePaths",  Anime		  Next		  		  Preferences.WriteDefaults		  Close		  		  'wndMain.CommonActions( "ReloadList" )		  CommonCore.ReloadList		End Sub	#tag EndEvent#tag EndEvents#tag Events btnBack	#tag Event		Sub Action()		  ppSplash.Value = ppSplash.Value - 1		End Sub	#tag EndEvent#tag EndEvents#tag Events lstArray	#tag Event		Sub Change()		  arMediaLists(index).RemoveEnabled = me.ListIndex > -1		End Sub	#tag EndEvent	#tag Event		Sub Open()		  me.AcceptFileDrop("special/folder")		End Sub	#tag EndEvent	#tag Event		Function KeyDown(Key As String) As Boolean		  If Keyboard.AsyncKeyDown( &h33 ) or Keyboard.AsyncKeyDown( &h75 ) then		    If me.ListIndex > -1 then me.RemoveRow( me.ListIndex )		  End If		End Function	#tag EndEvent	#tag Event		Sub DropObject(obj As DragItem, action As Integer)		  While obj.FolderItemAvailable		    me.AddRow "     " + obj.FolderItem.Name, obj.FolderItem.AbsolutePath		    me.RowPicture( me.LastIndex ) = obj.FolderItem.Icon(48)		    If not obj.NextItem then Exit		  Wend		End Sub	#tag EndEvent#tag EndEvents#tag Events arMediaLists	#tag Event		Sub ActionRemove()		  if lstArray(index).ListIndex > -1 then lstArray(index).RemoveRow( lstArray(index).ListIndex )		End Sub	#tag EndEvent	#tag Event		Sub ActionAdd()		  Dim dlg as new SelectFolderDialog		  Dim f as FolderItem = dlg.ShowModal		  Dim lstbox as Listbox = lstArray(index)		  		  If f <> nil then		    lstbox.AddRow "     " + f.Name, f.AbsolutePath		    lstbox.RowPicture( lstbox.LastIndex ) = f.Icon(48)		  End If		  		Exception err as NilObjectException		  MsgBox "File not found."		End Sub	#tag EndEvent#tag EndEvents#tag Events popJukeBox	#tag Event		Sub Change()		  		  Prefs.setStringValue "MediaPlayer", Me.Text		  		  Select Case Me.Text		    		  Case "XBMC"		    cvsMediaPlayer.Backdrop = XBMC		    		    Prefs.setStringValue "FileNamePoster",   "<movie>.jpg"		    Prefs.setStringValue "FileNameFanart",   "<movie>-fanart.jpg"		    Prefs.setStringValue "FileNameNFO",      "<movie>.nfo"		    Prefs.setStringValue "FileNameTrailer",  "<movie>-trailer"		    		    Prefs.setStringValue "FileNameNFOTV",    "tvshow.nfo"		    Prefs.setStringValue "FileNameNFOAnime", "tvshow.nfo"		    		  Case "YAMJ"		    cvsMediaPlayer.Backdrop = YAMJ		    		    Prefs.setStringValue "FileNamePoster",   "<movie>.jpg"		    Prefs.setStringValue "FileNameFanart",   "<movie>.fanart.jpg"		    Prefs.setStringValue "FileNameNFO",      "movie.nfo"		    Prefs.setStringValue "FileNameTrailer",  "<movie>[trailer]"		    		    Prefs.setStringValue "FileNameNFOTV",    "Set_<show>_1.nfo"		    Prefs.setStringValue "FileNameNFOAnime", "Set_<show>_1.nfo"		    		    		  Case "Boxee"		    cvsMediaPlayer.Backdrop = Boxee		    		    Prefs.setStringValue "FileNamePoster",   "<movie>.jpg"		    Prefs.setStringValue "FileNameFanart",   "<movie>-fanart.jpg"		    Prefs.setStringValue "FileNameNFO",      "<movie>.nfo"		    Prefs.setStringValue "FileNameTrailer",  "<movie>-trailer"		    		    Prefs.setStringValue "FileNameNFOTV",    "tvshow.nfo"		    Prefs.setStringValue "FileNameNFOAnime", "tvshow.nfo"		    		  End Select		End Sub	#tag EndEvent	#tag Event		Sub Open()		  Me.ListIndex = 0		End Sub	#tag EndEvent#tag EndEvents